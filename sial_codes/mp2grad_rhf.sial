      PROC S1TRANS
#     ------------
#
# Contract with the derivative integrals
# --------------------------------------
#
      PARDO mu, nu
#
            GET            W2_ao(mu,nu)
            GET            WHFa(mu,nu)
#
            Tpq(mu,nu)   = W2_ao(mu,nu)
            Tpq(mu,nu)  += WHFa(mu,nu)
            Tpq(mu,nu)  *= 2.0  
#
            EXECUTE SCONT1 Tpq(mu,nu)
#
      ENDPARDO mu, nu
#
# -----------------------------------------------------------------------------
#
      ENDPROC S1TRANS
