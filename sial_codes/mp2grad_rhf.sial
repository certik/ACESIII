         PROC UPDATE_PAI
#        ---------------
#
         PARDO a, i
#
               GET                        Painew_aa(a,i)
               GET                        Paiold_aa(a,i)
               Tai(a,i)                 = Painew_aa(a,i)
               execute energy_denominator Tai(a,i)
               Tai(a,i)                -= Paiold_aa(a,i)
#
               if kiter == 1
                  PUT e1ai(a,i) = tai(a,i)
               endif
#
               if kiter == 2
                  PUT e2ai(a,i) = tai(a,i)
               endif
#
               if kiter == 3
                  PUT e3ai(a,i) = tai(a,i)
               endif
#
               if kiter == 4
                  PUT e4ai(a,i) = tai(a,i)
               endif
#
               if kiter >= 5
                  PUT e5ai(a,i) = tai(a,i)
               endif
#
         ENDPARDO a, i
#
         ENDPROC UPDATE_PAI
