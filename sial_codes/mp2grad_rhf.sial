      PROC D1TRANS
#     ------------
#
# Contract with the derivative integrals
# --------------------------------------
#
      PARDO mu, nu
#
            GET            P2A_ao(mu,nu)
            GET            DHFA(mu,nu)
#
            Tpq(mu,nu)   = DHFA(mu,nu)
            Tpq(mu,nu)  += P2A_ao(mu,nu)
            Tpq(mu,nu)  *= 2.0  
#
            EXECUTE HCONT1 Tpq(mu,nu)
#
      ENDPARDO mu, nu
#
# -----------------------------------------------------------------------------
#
      ENDPROC D1TRANS
