           txx(mu,nu)  += t1xx(mu,nu) 
#
           t1xx(mu,nu)  = F3a(mu,nu) 
           t1xx(mu,nu) *= c3  
           txx(mu,nu)  += t1xx(mu,nu) 
#
           t1xx(mu,nu)  = F4a(mu,nu) 
           t1xx(mu,nu) *= c4  
           txx(mu,nu)  += t1xx(mu,nu) 
#
           t1xx(mu,nu)  = F5a(mu,nu) 
           t1xx(mu,nu) *= c5  
           txx(mu,nu)  += t1xx(mu,nu) 
#
           t1xx(mu,nu)  = F6a(mu,nu) 
