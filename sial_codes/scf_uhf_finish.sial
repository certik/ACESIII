           txx(mu,nu)  += t1xx(mu,nu) 
#
           t1xx(mu,nu)  = F10b(mu,nu) 
           t1xx(mu,nu) *= c10  
           txx(mu,nu)  += t1xx(mu,nu) 
#
           Fpq_b(mu,nu) = txx(mu,nu) 
#
       ENDPARDO mu, nu 
#
       execute sip_barrier 
#
#      Move histories 
#      -------------- 
#
#      2 --> 1 
#      ------- 
#
       PARDO mu, nu 
             GET F2a(mu,nu) 
             PUT F1a(mu,nu) = F2a(mu,nu)  
       ENDPARDO mu, nu 
#
       PARDO mu, nu 
             GET F2b(mu,nu) 
