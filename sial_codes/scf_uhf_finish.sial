           t1xx(mu,nu)  = F1b(mu,nu) 
           t1xx(mu,nu) *= c1  
           txx(mu,nu)   = t1xx(mu,nu) 
#
           t1xx(mu,nu)  = F2b(mu,nu) 
           t1xx(mu,nu) *= c2  
           txx(mu,nu)  += t1xx(mu,nu) 
#
           t1xx(mu,nu)  = F3b(mu,nu) 
           t1xx(mu,nu) *= c3  
           txx(mu,nu)  += t1xx(mu,nu) 
#
           t1xx(mu,nu)  = F4b(mu,nu) 
