              #GET                          Uyaa(j,j2) 
               tppqq(a,a1,j2,j1)          = Vaajj(a,a1,j,j1)*SUyaa(j,j2) 
               PREPARE VYaajj(a,a1,j2,j1) += tppqq(a,a1,j2,j1) 
#
               t1ppqq(a,a1,j1,j2)          = tppqq(a,a1,j2,j1)
               PREPARE VYaajj(a,a1,j1,j2) += t1ppqq(a,a1,j1,j2) 
