      ENDPARDO a, i, i1, i2 
#
      PARDO a, i, j, j1 
#
            REQUEST          Vpiqj(a,i,j1,j) a 
            GET              Sxa(j,j1)
            tai(a,i)       = Vpiqj(a,i,j1,j)*Sxa(j,j1)
            tai(a,i)      *= -1.0  
            PUT Qxai(a,i) += tai(a,i)
