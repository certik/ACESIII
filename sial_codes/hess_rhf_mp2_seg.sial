                  T4aiai(a,i,a1,i1) = Viaai(i,a,a1,i1)
#
                  Taiai(a,i,a1,i1) -= T3aiai(a,i,a1,i1)
                  Taiai(a,i,a1,i1) += T4aiai(a,i,a1,i1)
