              #GET                          Uyaa(j,b)
               tppqq(a,a1,b,j1)           = Vaajj(a,a1,j,j1)*SUyaa(j,b) 
               PREPARE VYaabj(a,a1,b,j1) += tppqq(a,a1,b,j1) 
