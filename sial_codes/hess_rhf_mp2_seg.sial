#
      PARDO a, i, i1 
#
            GET              Sxa(a,i1)
            tai(a,i)       = Sxa(a,i1)*Fock_a(i,i1)
            tai(a,i)      *= -1.0  
            PUT Qxai(a,i) += tai(a,i)
