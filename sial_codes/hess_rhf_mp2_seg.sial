# ---------------------------------------------------------------------- 
#
      PROC PART3 
#     ---------- 
#
      PARDO i, i1 
#
            GET       Sxa(i,i1) 
            GET       DIii(i,i1) 
#
            etemp   = Sxa(i,i1)*DIii(i,i1)
            etemp  *= 2.0 
            hessxy += etemp 
#
      ENDPARDO i, i1 
#
      PARDO a, a1 
#
            GET       Sxa(a,a1) 
            GET       DIaa(a,a1) 
#
            etemp   = Sxa(a,a1)*DIaa(a,a1)
            etemp  *= 2.0 
            hessxy += etemp 
#
      ENDPARDO a, a1 
#
      PARDO a, i1 
#
            GET       Sxa(a,i1) 
            GET       DIai(a,i1) 
#
            etemp   = Sxa(a,i1)*DIai(a,i1)
            etemp  *= 4.0 
            hessxy += etemp 
#
      ENDPARDO a, i1 
#
      ENDPROC PART3 
