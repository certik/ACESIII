      ENDPARDO a, i 
#
      execute sip_barrier
#
      DO kiter
#
         esum = 0.0
         enew = 0.0
#
         PARDO a, i
#
               GET        DXai(a,i)
               tai(a,i) = DXai(a,i)   
#
               DO a1
