            tppss(a,i,iatom,ix)     = Cainew(a,i,iatom,ix) 
            tppss(a,i,iatom,ix)    += STa(a,i,iatom,ix)  
            t1ppss(i,a,iatom,ix)    = tppss(a,i,iatom,ix)  
            t1ppss(i,a,iatom,ix)   *= -1.0  
