#
     PARDO a, a1, a2, i
           REQUEST                     Vaaai(a,a1,a2,i) i
           REQUEST                     Vaaai(a2,a1,a,i) i
           tpppp(a,a1,a2,i)          = Vaaai(a2,a1,a,i)
           tpppp(a,a1,a2,i)         -= Vaaai(a,a1,a2,i)
           tpppp(a,a1,a2,i)         *= -1.0
           PREPARE VSaaai(a,a1,a2,i) = tpppp(a,a1,a2,i)
     ENDPARDO a, a1, a2, i

