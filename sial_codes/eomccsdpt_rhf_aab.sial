      execute list_to_blocks                t1b_old
      execute list_to_blocks                TSaiai
      execute list_to_blocks                T2aiai
      execute list_to_blocks                TSbjbj

      execute list_to_blocks                A1a
      execute list_to_blocks                A2aa
      execute list_to_blocks                A2ab

