      ENDPARDO j, j1, j2, j3 
      execute print_scalar esum 
#
      esum = 0.0 
      PARDO i, i1, j, j1 
#
            REQUEST Dijkl_ab(i,j,i1,j1) i  
            etemp = Dijkl_ab(i,j,i1,j1)*Dijkl_ab(i,j,i1,j1)
