            PUT Iai_bb(b,j) += Tbj(b,j) 
#
      ENDPARDO sigma, b, j 
#
      PARDO sigma, b, j 
#
            GET I1xj(sigma,j) 
            Tbj(b,j)  = I1xj(sigma,j)*cb(sigma,b) 
            Tbj(b,j) *= -0.5 
            PUT Iai_bb(b,j) += Tbj(b,j) 
#
      ENDPARDO sigma, b, j 
      execute sip_barrier 
      delete Ixj  
      delete I1xj  
#
      ENDPROC IAIBB1 
