     PROC T2NEW_ZERO
#    --------------- 
#
#     Zero out new 2-particle amplitude arrays 
#     ---------------------------------------- 
#
          PARDO a, b, i, j 
                Taibj(a,i,b,j) = 0.0
                PREPARE T2new_ab(a,i,b,j) = Taibj(a,i,b,j) 
          ENDPARDO a, b, i, j 
#
#    Zero-out intermediate arrays
#    ---------------------------------------------
#
          PARDO mu, nu, i, j
                Txixi(mu,i,nu,j)           = 0.0
                PREPARE TAO_ab(mu,i,nu,j)  = Txixi(mu,i,nu,j)
                PREPARE T2AO_ab(mu,i,nu,j) = Txixi(mu,i,nu,j)
          ENDPARDO mu, nu, i, j
#
     ENDPROC T2NEW_ZERO
