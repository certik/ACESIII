      PROC WminjAB
#     ------------
#
      PARDO a, b, i1, j1 
#
            REQUEST T2old_ab(a,i1,b,j1) j1
#
            DO i
            DO j
#
               REQUEST                      Vpiqj(i1,i,j1,j) j
               T1aibj(a,i,b,j)            = T2old_ab(a,i1,b,j1)*Vpiqj(i1,i,j1,j)
               PREPARE T2new_ab(a,i,b,j) += T1aibj(a,i,b,j)
#
            ENDDO j
            ENDDO i
#
      ENDPARDO a, b, i1, j1
#
      ENDPROC WminjAB
