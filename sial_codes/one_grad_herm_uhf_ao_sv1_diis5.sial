            DO mu 
#
               Txbaj(mu,b,a1,j) = Dabci_abab(a,b,a1,j)*ca(mu,a) 
#
               DO nu 
#
                  Txxaj(mu,nu,a1,j) = Txbaj(mu,b,a1,j)*cb(nu,b) 
                  PREPARE Ixxaj(mu,nu,a1,j) += Txxaj(mu,nu,a1,j) 
