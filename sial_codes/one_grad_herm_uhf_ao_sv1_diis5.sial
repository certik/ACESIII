      PROC S2IAB 
#
           DO a 
           DO a1 
              GET Iab_aa(a,a1) 
              execute dump_block Iab_aa(a,a1)  
           ENDDO a1 
           ENDDO a 
#
           DO b 
           DO b1 
              GET Iab_bb(b,b1) 
              execute dump_block Iab_bb(b,b1)  
           ENDDO b1 
           ENDDO b 
