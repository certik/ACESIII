      ENDPROC S2IAB 
#
      PROC S2IAI 
#
           DO a 
           DO i 
              GET Iai_aa(a,i) 
              execute dump_block Iai_aa(a,i)  
           ENDDO i 
           ENDDO a 
#
           DO b 
           DO j 
              GET Iai_bb(b,j) 
              execute dump_block Iai_bb(b,j)  
