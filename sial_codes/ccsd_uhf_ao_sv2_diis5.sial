            tai(a,i)         = t1a_new(a,i) 
            t1ai(a,i)        = t1a_old(a,i) 
            execute energy_denominator tai 
            PUT t1a_old(a,i) = tai(a,i) 
            tai(a,i)        -= t1ai(a,i) 
#
            if kiter == 1 
               PUT e1ai(a,i) = Tai(a,i) 
