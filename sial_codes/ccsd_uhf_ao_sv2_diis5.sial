#
            if kiter == 2 
               PUT e2ai(a,i) = Tai(a,i) 
            endif 
#
            if kiter == 3 
               PUT e3ai(a,i) = Tai(a,i) 
            endif 
#
            if kiter == 4 
               PUT e4ai(a,i) = Tai(a,i) 
            endif 
#
            if kiter >= 5 
               PUT e5ai(a,i) = Tai(a,i) 
            endif 
#
      ENDPARDO a, i 
#
      PARDO b, j 
#
            GET                t1b_new(b,j) 
            GET                t1b_old(b,j) 
            tbj(b,j)         = t1b_new(b,j) 
            t1bj(b,j)        = t1b_old(b,j) 
            execute energy_denominator tbj
            PUT t1b_old(b,j) = tbj(b,j) 
            tbj(b,j)        -= t1bj(b,j) 
#
            if kiter == 1 
               PUT e1bj(b,j) = Tbj(b,j) 
            endif 
#
            if kiter == 2 
               PUT e2bj(b,j) = Tbj(b,j) 
            endif 
#
            if kiter == 3 
               PUT e3bj(b,j) = Tbj(b,j) 
            endif 
#
            if kiter == 4 
               PUT e4bj(b,j) = Tbj(b,j) 
            endif 
#
            if kiter >= 5 
               PUT e5bj(b,j) = Tbj(b,j) 
            endif 
#
      ENDPARDO b, j 
      execute sip_barrier 
#
      ENDPROC UPDATET1 
