#
# PPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPP 
#
      PROC WMEBJAB 
#     ------------

      create D2new_bb 
      execute server_barrier 
#
# Contribution to T2new_bb 
# ------------------------ 
#
      PARDO b, j, a, i 
#
            REQUEST              TAUP_ab(a,i,b,j)    j  
#
            DO j1 
            DO b1 
#
               REQUEST              T2old_ab(a,i,b1,j1) j1  
               R1bjbj(b1,j1,b,j)  = T2old_ab(a,i,b1,j1)*TAUP_ab(a,i,b,j) 
               PUT D2new_bb(b1,j1,b,j) += R1bjbj(b1,j1,b,j) 
            ENDDO b1 
            ENDDO j1 
#
      ENDPARDO b, j, a, i
      execute server_barrier 
#
      PARDO b, j, b1, j1 
            GET                            D2new_bb(b1,j1,b,j) 
#
            R1bjbj(b1,j1,b,j)            = D2new_bb(b1,j1,b,j)  
            R2bjbj(b1,j,b,j1)            = R1bjbj(b1,j1,b,j) 
            R2bjbj(b1,j,b,j1)           *= -1.0  
            R3bjbj(b,j1,b1,j)            = R1bjbj(b1,j1,b,j) 
            R3bjbj(b,j1,b1,j)           *= -1.0  
            R4bjbj(b,j,b1,j1)            = R1bjbj(b1,j1,b,j) 
#
            PREPARE T2new_bb(b1,j1,b,j) += R1bjbj(b1,j1,b,j) 
            PREPARE T2new_bb(b1,j,b,j1) += R2bjbj(b1,j,b,j1) 
            PREPARE T2new_bb(b,j1,b1,j) += R3bjbj(b,j1,b1,j) 
            PREPARE T2new_bb(b,j,b1,j1) += R4bjbj(b,j,b1,j1) 
      ENDPARDO b, j, b1, j1 
#
# Contribution to T2new_ab 
# ------------------------ 
#
      PARDO i1, a1, a, i
#
            REQUEST              T2old_aa(a1,i1,a,i) i 
#
            DO j 
            DO b 
#
               REQUEST              TAUP_ab(a,i,b,j)    j  
#
               R1aibj(a1,i1,b,j)  = T2old_aa(a1,i1,a,i)*TAUP_ab(a,i,b,j)  
               PREPARE T2new_ab(a1,i1,b,j) += R1aibj(a1,i1,b,j) 
            ENDDO b 
            ENDDO j 
#
      ENDPARDO i1, a1, a, i
      execute server_barrier 
      delete D2new_bb 
#
      ENDPROC WMEBJAB 
#     ---------------
#
# PPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPP 
#
# PPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPP 
#
      PROC WMEBJBA  
#     ------------
#
      create D2new_aa 
      execute server_barrier 
#
# Contribution to T2new_aa 
# ------------------------ 
#
      PARDO a, i, b, j
#
            REQUEST               TAUP1_ab(a,i,b,j)    j  
#
            DO a1 
            DO i1 
#
               REQUEST               T2old_ab(a1,i1,b,j) j  
               R1aiai(a1,i1,a,i)  =  T2old_ab(a1,i1,b,j)*TAUP1_ab(a,i,b,j) 
               PUT D2new_aa(a1,i1,a,i) += R1aiai(a1,i1,a,i) 
            ENDDO i1 
            ENDDO a1 
#
      ENDPARDO a, i, b, j
      execute server_barrier 
#
      PARDO a, i, a1, i1 
            GET                            D2new_aa(a1,i1,a,i) 
            R1aiai(a1,i1,a,i)            = D2new_aa(a1,i1,a,i) 
            R2aiai(a1,i,a,i1)            =  R1aiai(a1,i1,a,i) 
            R2aiai(a1,i,a,i1)           *= -1.0  
            R3aiai(a,i1,a1,i)            =  R1aiai(a1,i1,a,i) 
            R3aiai(a,i1,a1,i)           *= -1.0  
            R4aiai(a,i,a1,i1)            =  R1aiai(a1,i1,a,i) 
#
            PREPARE T2new_aa(a1,i1,a,i) += R1aiai(a1,i1,a,i) 
            PREPARE T2new_aa(a1,i,a,i1) += R2aiai(a1,i,a,i1) 
            PREPARE T2new_aa(a,i1,a1,i) += R3aiai(a,i1,a1,i) 
            PREPARE T2new_aa(a,i,a1,i1) += R4aiai(a,i,a1,i1) 
      ENDPARDO a, i, a1, i1 
#
# Contribution to T2new_ab 
# ------------------------ 
#
      PARDO a, i, b, j  
#
            REQUEST              TAUP1_ab(a,i,b,j)    j  
#
            DO j1 
            DO b1
#
               REQUEST              T2old_bb(b1,j1,b,j) j 
#
               R1aibj(a,i,b1,j1)  = T2old_bb(b1,j1,b,j)*TAUP1_ab(a,i,b,j)  
               PREPARE T2new_ab(a,i,b1,j1) += R1aibj(a,i,b1,j1) 
            ENDDO b1
            ENDDO j1 
#
      ENDPARDO a, i, b, j  
      execute server_barrier 
      delete D2new_aa 
#
      ENDPROC WMEBJBA  
#     ---------------
#
# -------------------------------------------------------------------------------  
#
#
# -------------------------------------------------------------------------------  
#
# Compute contributions coming from -t^a_m t^e_i (V^{mb}_{ej)-V^{mb}_{je})
#
      PROC T2AAS2
#     -----------
#
      PARDO i, i1, a2, i2
#
               T1iiai(i,i1,a2,i2) = 0.0
               T2iiai(i2,i1,a2,i) = 0.0
#
               DO a1
#
                  REQUEST Viaai(i1,a1,a2,i2) i2
                  REQUEST Vaaii(a2,a1,i1,i2) i2
                  REQUEST Viaai(i1,a1,a2,i)  i
                  REQUEST Vaaii(a2,a1,i1,i)  i
                 #GET     t1a_old(a1,i2)
                 #GET     t1a_old(a1,i)
#
                  Tpppp(a1,i1,a2,i2)  = Viaai(i1,a1,a2,i2)
                  T1pppp(a1,i1,a2,i2) = Vaaii(a2,a1,i1,i2)
                  Tpppp(a1,i1,a2,i2) -= T1pppp(a1,i1,a2,i2)
#
                  Tiiai(i,i1,a2,i2)   = Tpppp(a1,i1,a2,i2)*Lt1aR(a1,i)
                  T1iiai(i,i1,a2,i2) -= Tiiai(i,i1,a2,i2)
#
                  Tpppp(a1,i1,a2,i)   = Viaai(i1,a1,a2,i)
                  T1pppp(a1,i1,a2,i)  = Vaaii(a2,a1,i1,i)
                  Tpppp(a1,i1,a2,i)  -= T1pppp(a1,i1,a2,i)
#
                  Tiiai(i2,i1,a2,i)   = Tpppp(a1,i1,a2,i)*Lt1aR(a1,i2)
                  T2iiai(i2,i1,a2,i) += Tiiai(i2,i1,a2,i)
#
               ENDDO a1
#
#              Compute contribution to amplitude
#              ---------------------------------
               DO a
#
                 #GET                  t1a_old(a,i1)
                  Tiiai(i,i1,a2,i2)  = T2iiai(i2,i1,a2,i)
                  Tiiai(i,i1,a2,i2) += T1iiai(i,i1,a2,i2)
                  T1aiai(a,i,a2,i2)  = Tiiai(i,i1,a2,i2)*Lt1aR(a,i1)
                  PREPARE T2new_aa(a,i,a2,i2) += T1aiai(a,i,a2,i2)
#
               ENDDO a
#
      ENDPARDO i, i1, a2, i2
#
      PARDO i, i1, a, i2
#
            T3iiai(i,i1,a,i2) = 0.0
#
            DO a1
#
               REQUEST Viaai(i1,a1,a,i2) i2
               REQUEST Vaaii(a,a1,i1,i2) i2
              #GET t1a_old(a1,i)
#
               Tpppp(a1,i1,a,i2)  = Viaai(i1,a1,a,i2)
               T1pppp(a1,i1,a,i2) = Vaaii(a,a1,i1,i2)
               Tpppp(a1,i1,a,i2) -= T1pppp(a1,i1,a,i2)
               T1iiai(i,i1,a,i2) = Tpppp(a1,i1,a,i2)*Lt1aR(a1,i)
               T3iiai(i,i1,a,i2) += T1iiai(i,i1,a,i2)
#
            ENDDO a1
#
            DO a2
#
              #GET                            t1a_old(a2,i1)
               T1aiai(a,i,a2,i2)            = T3iiai(i,i1,a,i2)*Lt1aR(a2,i1)
               PREPARE T2new_aa(a,i,a2,i2) += T1aiai(a,i,a2,i2)
#
            ENDDO a2
#
            T4iiai(i2,i1,a,i) = 0.0
#
            DO a1
#
               REQUEST Viaai(i1,a1,a,i) i
               REQUEST Vaaii(a,a1,i1,i) i
               Tpppp(a1,i1,a,i)  = Viaai(i1,a1,a,i)
               T1pppp(a1,i1,a,i) = Vaaii(a,a1,i1,i)
               Tpppp(a1,i1,a,i) -= T1pppp(a1,i1,a,i)
#
              #GET t1a_old(a1,i2)
               T1iiai(i2,i1,a,i) = Tpppp(a1,i1,a,i)*Lt1aR(a1,i2)
               T4iiai(i2,i1,a,i) += T1iiai(i2,i1,a,i)
#
            ENDDO a1
#
            DO a2
#
              #GET                            t1a_old(a2,i1)
               T1aiai(a,i,a2,i2)            = T4iiai(i2,i1,a,i)*Lt1aR(a2,i1)
               T1aiai(a,i,a2,i2)           *= -1.0
               PREPARE T2new_aa(a,i,a2,i2) += T1aiai(a,i,a2,i2)
#
            ENDDO a2
#
      ENDPARDO i, i1, a, i2
#
      ENDPROC T2AAS2
#     --------------
#
# -------------------------------------------------------------------------------
#
# Compute contributions coming from -t^a_m t^e_i (V^{mb}_{ej)-V^{mb}_{je})
#
      PROC T2BBS2
#     -----------
#
      PARDO j, j1, b2, j2
#
               T1jjbj(j,j1,b2,j2) = 0.0
               T2jjbj(j2,j1,b2,j) = 0.0
#
               DO b1
#
                  REQUEST Vjbbj(j1,b1,b2,j2) j2
                  REQUEST Vbbjj(b2,b1,j1,j2) j2
                  REQUEST Vjbbj(j1,b1,b2,j)  j
                  REQUEST Vbbjj(b2,b1,j1,j)  j
                 #GET     t1b_old(b1,j2)
                 #GET     t1b_old(b1,j)
#
                  Tqqqq(b1,j1,b2,j2)  = Vjbbj(j1,b1,b2,j2)
                  T1qqqq(b1,j1,b2,j2) = Vbbjj(b2,b1,j1,j2)
                  Tqqqq(b1,j1,b2,j2) -= T1qqqq(b1,j1,b2,j2)
#
                  Tjjbj(j,j1,b2,j2)   = Tqqqq(b1,j1,b2,j2)*Lt1bR(b1,j)
                  T1jjbj(j,j1,b2,j2) -= Tjjbj(j,j1,b2,j2)
#
                  Tqqqq(b1,j1,b2,j)   = Vjbbj(j1,b1,b2,j)
                  T1qqqq(b1,j1,b2,j)  = Vbbjj(b2,b1,j1,j)
                  Tqqqq(b1,j1,b2,j)  -= T1qqqq(b1,j1,b2,j)
#
                  Tjjbj(j2,j1,b2,j)   = Tqqqq(b1,j1,b2,j)*Lt1bR(b1,j2)
                  T2jjbj(j2,j1,b2,j) += Tjjbj(j2,j1,b2,j)
#
               ENDDO b1
#
#              Compute contribution to amplitude
#              ---------------------------------
               DO b
#
                 #GET                  t1b_old(b,j1)
                  Tjjbj(j,j1,b2,j2)  = T2jjbj(j2,j1,b2,j)
                  Tjjbj(j,j1,b2,j2) += T1jjbj(j,j1,b2,j2)
                  T1bjbj(b,j,b2,j2)  = Tjjbj(j,j1,b2,j2)*Lt1bR(b,j1)
                  PREPARE T2new_bb(b,j,b2,j2) += T1bjbj(b,j,b2,j2)
#
               ENDDO b
#
      ENDPARDO j, j1, b2, j2
#
      PARDO j, j1, b, j2
#
            T3jjbj(j,j1,b,j2) = 0.0
#
            DO b1
#
               REQUEST Vjbbj(j1,b1,b,j2) j2
               REQUEST Vbbjj(b,b1,j1,j2) j2
              #GET t1b_old(b1,j)
#
               Tqqqq(b1,j1,b,j2)  = Vjbbj(j1,b1,b,j2)
               T1qqqq(b1,j1,b,j2) = Vbbjj(b,b1,j1,j2)
               Tqqqq(b1,j1,b,j2) -= T1qqqq(b1,j1,b,j2)
               T1jjbj(j,j1,b,j2) = Tqqqq(b1,j1,b,j2)*Lt1bR(b1,j)
               T3jjbj(j,j1,b,j2) += T1jjbj(j,j1,b,j2)
#
            ENDDO b1
#
            DO b2
#
              #GET                            t1b_old(b2,j1)
               T1bjbj(b,j,b2,j2)            = T3jjbj(j,j1,b,j2)*Lt1bR(b2,j1)
               PREPARE T2new_bb(b,j,b2,j2) += T1bjbj(b,j,b2,j2)
#
            ENDDO b2
            T4jjbj(j2,j1,b,j) = 0.0
#
            DO b1
#
               REQUEST Vjbbj(j1,b1,b,j) j
               REQUEST Vbbjj(b,b1,j1,j) j
               Tqqqq(b1,j1,b,j)  = Vjbbj(j1,b1,b,j)
               T1qqqq(b1,j1,b,j) = Vbbjj(b,b1,j1,j)
               Tqqqq(b1,j1,b,j) -= T1qqqq(b1,j1,b,j)
#
              #GET t1b_old(b1,j2)
               T1jjbj(j2,j1,b,j) = Tqqqq(b1,j1,b,j)*Lt1bR(b1,j2)
               T4jjbj(j2,j1,b,j) += T1jjbj(j2,j1,b,j)
#
            ENDDO b1
#
            DO b2
#
              #GET                            t1b_old(b2,j1)
               T1bjbj(b,j,b2,j2)            = T4jjbj(j2,j1,b,j)*Lt1bR(b2,j1)
               T1bjbj(b,j,b2,j2)           *= -1.0
               PREPARE T2new_bb(b,j,b2,j2) += T1bjbj(b,j,b2,j2)
#
            ENDDO b2
#
      ENDPARDO j, j1, b, j2
#
      ENDPROC T2BBS2
#     --------------
#
      PROC T2ABS2 
#     ----------- 
#
      PARDO i1, i, b, j 
#
           Tiibj(i1,i,b,j) = 0.0 
#
           DO a1 
# 
              REQUEST Viabj(i1,a1,b,j) j  
             #GET t1a_old(a1,i) 
#
              T1iibj(i1,i,b,j) = Viabj(i1,a1,b,j)*Lt1aR(a1,i) 
              Tiibj(i1,i,b,j) += T1iibj(i1,i,b,j) 
#
           ENDDO a1  
#
           DO b1 
#
              REQUEST Vbbii(b,b1,i1,i) i  
             #GET t1b_old(b1,j) 
#
              T1iibj(i1,i,b,j) = Vbbii(b,b1,i1,i)*Lt1bR(b1,j)  
              Tiibj(i1,i,b,j) += T1iibj(i1,i,b,j) 
#
          ENDDO b1  
#
          DO a 
#
            #GET t1a_old(a,i1) 
             Taibj(a,i,b,j)  = Tiibj(i1,i,b,j)*Lt1aR(a,i1) 
             Taibj(a,i,b,j) *= -1.0 
             PREPARE T2new_ab(a,i,b,j) += Taibj(a,i,b,j) 
#
          ENDDO a 
#
      ENDPARDO i1, i, b, j 
#
      PARDO j1, i, a, j 
#
            Tjjai(j1,j,a,i) = 0.0 
#
            DO a1 
#
               REQUEST Vaajj(a,a1,j1,j) j  
              #GET t1a_old(a1,i) 
#
               T1jjai(j1,j,a,i) = Vaajj(a,a1,j1,j)*Lt1aR(a1,i) 
               Tjjai(j1,j,a,i) += T1jjai(j1,j,a,i)
#
            ENDDO a1 
#
            DO b1 
# 
               REQUEST Viabj(i,a,b1,j1) j1  
              #GET t1b_old(b1,j) 
#
               T1jjai(j1,j,a,i) = Viabj(i,a,b1,j1)*Lt1bR(b1,j) 
               Tjjai(j1,j,a,i) += T1jjai(j1,j,a,i) 
#
            ENDDO b1  
#
            DO b 
#
              #GET t1b_old(b,j1) 
               Taibj(a,i,b,j)  = Tjjai(j1,j,a,i)*Lt1bR(b,j1) 
               Taibj(a,i,b,j) *= -1.0 
               PREPARE T2new_ab(a,i,b,j) += Taibj(a,i,b,j) 
#
            ENDDO b 
#
      ENDPARDO j1, i, a, j 
#
      ENDPROC T2ABS2 
#     -------------- 
#
# PPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPP 
#
# Compute contributions from 'Wmjbe'. 
#
#
      PROC WMJBEab
#     -------------
#
      PARDO b, i, b1, i1
#
            REQUEST Vbbii(b,b1,i1,i) i
            Tiiqq(i1,i,b,b1) = 0.
#
            DO j1
#
               REQUEST Vpiqj(i,i1,b1,j1) j1
              #GET t1b_old(b,j1)
#
               tbj(b,j1)         = Lt1bR(b,j1)
               T1iiqq(i1,i,b,b1) = Vpiqj(i,i1,b1,j1)*tbj(b,j1)
               Tiiqq(i1,i,b,b1) -= T1iiqq(i1,i,b,b1)
#
            ENDDO j1
#
            T1iiqq(i1,i,b,b1) = Vbbii(b,b1,i1,i)
            Tiiqq(i1,i,b,b1) += T1iiqq(i1,i,b,b1)
            PREPARE Wiibb(i1,i,b,b1) = Tiiqq(i1,i,b,b1)
#
      ENDPARDO b, i, b1, i1
      execute server_barrier

      PARDO a1, b, i1, b1
         REQUEST Vbbai(b1,b,a1,i1) i1
         DO i
           #GET t1a_old(a1,i)
            T1iiqq(i1,i,b,b1) = Vbbai(b1,b,a1,i1)*Lt1aR(a1,i)
            PREPARE Wiibb(i1,i,b,b1)  += T1iiqq(i1,i,b,b1)
         ENDDO i
      ENDPARDO a1, b, i1, b1

      PARDO i, b, a1, j1
         REQUEST T2old_ab(a1,i,b,j1) j1
        #GET t1a_old(a1,i)
        #GET t1b_old(b,j1)

         Taibj(a1,i,b,j1)   = Lt1aR(a1,i)^Lt1bR(b,j1)
#
         DO i1
         DO b1
#
            REQUEST Vpiqj(a1,i1,b1,j1) j1
            Tiiqq(i1,i,b,b1) = 0.
#
            T1aibj(a1,i,b,j1)  = 0.5*T2old_ab(a1,i,b,j1)
            T1aibj(a1,i,b,j1) += Taibj(a1,i,b,j1)
#
            T1iiqq(i1,i,b,b1) = T1aibj(a1,i,b,j1)*Vpiqj(a1,i1,b1,j1)
            Tiiqq(i1,i,b,b1) -= T1iiqq(i1,i,b,b1)
#
            PREPARE Wiibb(i1,i,b,b1) += Tiiqq(i1,i,b,b1)
         ENDDO b1
         ENDDO i1
#
      ENDPARDO i, b, a1, j1
      execute server_barrier

#  Calculate contribution to the amplitudes
#  ----------------------------------------
#
      PARDO a, j, i1, b1
#
            REQUEST            T2old_ab(a,i1,b1,j) j
#
            DO i
            DO b
#
               REQUEST            Wiibb(i1,i,b,b1)    i
#
               Taibj(a,i,b,j) = 0.
               T1aibj(a,i,b,j)  = T2old_ab(a,i1,b1,j)*Wiibb(i1,i,b,b1)
               Taibj(a,i,b,j)  -= T1aibj(a,i,b,j)
#
               PREPARE T2new_ab(a,i,b,j) += Taibj(a,i,b,j)
            ENDDO b
            ENDDO i
#
      ENDPARDO a, j, i1, b1
#
      ENDPROC WMJBEab
#     ---------------
#
      PROC WMJBEba
#     -------------
#
      PARDO a, j, a1, j1
#
            REQUEST Vaajj(a,a1,j1,j) j
            Tjjpp(j1,j,a,a1) = 0.
#
            DO i1
#
               REQUEST Vpiqj(a1,i1,j,j1) j1
              #GET t1a_old(a,i1)
               T1jjpp(j1,j,a,a1) = Vpiqj(a1,i1,j,j1)*Lt1aR(a,i1)
               Tjjpp(j1,j,a,a1) -= T1jjpp(j1,j,a,a1)
            ENDDO i1
#
            T1jjpp(j1,j,a,a1) = Vaajj(a,a1,j1,j)
            Tjjpp(j1,j,a,a1) += T1jjpp(j1,j,a,a1)
            PREPARE Wjjaa(j1,j,a,a1) = Tjjpp(j1,j,a,a1)
#
      ENDPARDO a, j, a1, j1
      execute server_barrier

      PARDO a1, a, b1, j1
         REQUEST Vaabj(a1,a,b1,j1) j1

         DO j
           #GET t1b_old(b1,j)
            T1jjpp(j1,j,a,a1) = Vaabj(a1,a,b1,j1)*Lt1bR(b1,j)
            PREPARE Wjjaa(j1,j,a,a1) += T1jjpp(j1,j,a,a1)
         ENDDO j
      ENDPARDO a1, a, b1, j1

      PARDO a, j, i1, b1
            REQUEST T2old_ab(a,i1,b1,j) j
#
           #GET t1a_old(a,i1)
           #GET t1b_old(b1,j)
#
            Taibj(a,i1,b1,j)   = Lt1bR(b1,j)^Lt1aR(a,i1)
            T1aibj(a,i1,b1,j)  = 0.5*T2old_ab(a,i1,b1,j)
            T1aibj(a,i1,b1,j) += Taibj(a,i1,b1,j)
#
            DO j1
            DO a1
#
               REQUEST Vpiqj(a1,i1,b1,j1) j1
#
               Tjjpp(j1,j,a,a1)  = 0.
               T1jjpp(j1,j,a,a1) = T1aibj(a,i1,b1,j)*Vpiqj(a1,i1,b1,j1)
               Tjjpp(j1,j,a,a1) -= T1jjpp(j1,j,a,a1)
#
               PREPARE Wjjaa(j1,j,a,a1) += Tjjpp(j1,j,a,a1)
            ENDDO a1
            ENDDO j1
#
      ENDPARDO a, j, i1, b1
      execute server_barrier

#  Calculate contribution to the amplitudes
#  ----------------------------------------
#
      PARDO i, b, a1, j1
#
            REQUEST           T2old_ab(a1,i,b,j1) j1
#
            DO a
            DO j
#
               REQUEST           Wjjaa(j1,j,a,a1)    j1
#
               Taibj(a,i,b,j) = 0.
               T1aibj(a,i,b,j) = Wjjaa(j1,j,a,a1)*T2old_ab(a1,i,b,j1)
               Taibj(a,i,b,j) -= T1aibj(a,i,b,j)
#
               PREPARE T2new_ab(a,i,b,j) += Taibj(a,i,b,j)
            ENDDO j
            ENDDO a
#
      ENDPARDO i, b, a1, j1
#
      ENDPROC WMJBEba
#     ---------------
#
# SSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSS 
#
      PROC WMEBJ 
# 
           CALL WMEBJAA
           CALL WMEBJBB
           CALL WMEBJAB
           CALL WMEBJBA
           CALL WMJBEab
           CALL WMJBEba 
           CALL T2AAS2
           CALL T2BBS2
           CALL T2ABS2
#
      ENDPROC WMEBJ 
#
# SSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSSS 
#
# PPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPP 
#
#    ------------------------------------------------------------------------ 
#
     PROC ENERGY_NEW 
# 
#    ------------------------------------------------------------------------ 
#
     esuma   = 0.0 
     ecorraa = 0.0 
     PARDO a, a1, i, i1 
#
           REQUEST VSpipi(a,i,a1,i1) i1  
           REQUEST T2old_aa(a,i,a1,i1) i1  
#
          #GET t1a_old(a1,i1) 
          #GET t1a_old(a1,i) 
          #GET t1a_old(a,i1) 
          #GET t1a_old(a,i) 
#
           tai(a1,i1)         =          Lt1aR(a1,i1) 
           T2aiai(a,i,a1,i1)  = Lt1aR(a,i)^tai(a1,i1)
           tai(a,i1)          =           Lt1aR(a,i1) 
           T3aiai(a,i,a1,i1)  = Lt1aR(a1,i)^tai(a,i1)
           T2aiai(a,i,a1,i1) -= T3aiai(a,i,a1,i1)
           T2aiai(a,i,a1,i1) += T2old_aa(a,i,a1,i1)
#
           etemp = VSpipi(a,i,a1,i1)*T2aiai(a,i,a1,i1) 
           etemp = 0.25*etemp 
           esuma += etemp 
#
     ENDPARDO a, a1, i, i1  
#
     ecorrbb = 0.0 
     esumb   = 0.0 
     PARDO b, b1, j, j1 
#
           REQUEST VSqjqj(b,j,b1,j1) j1  
           REQUEST T2old_bb(b,j,b1,j1) j1  
#
          #GET t1b_old(b1,j1) 
          #GET t1b_old(b1,j) 
          #GET t1b_old(b,j1) 
          #GET t1b_old(b,j) 
#
           tbj(b1,j1)         =          Lt1bR(b1,j1) 
           T2bjbj(b,j,b1,j1)  = Lt1bR(b,j)^tbj(b1,j1)
           tbj(b,j1)          =           Lt1bR(b,j1) 
           T3bjbj(b,j,b1,j1)  = Lt1bR(b1,j)^tbj(b,j1)
           T2bjbj(b,j,b1,j1) -= T3bjbj(b,j,b1,j1)
           T2bjbj(b,j,b1,j1) += T2old_bb(b,j,b1,j1)
#
           etemp = VSqjqj(b,j,b1,j1)*T2bjbj(b,j,b1,j1) 
           etemp = 0.25*etemp 
#          execute print_scalar etemp 
           esumb += etemp 
#
     ENDPARDO b, b1, j, j1 

     ecorrab = 0.0 
     esumab  = 0.0 
     PARDO a, b, i, j 
#
           REQUEST Vpiqj(a,i,b,j) j  
           REQUEST T2old_ab(a,i,b,j) j  
          #GET t1a_old(a,i) 
          #GET t1b_old(b,j) 
#
           Taibj(a,i,b,j)  = Lt1aR(a,i)^Lt1bR(b,j) 
           Taibj(a,i,b,j) += T2old_ab(a,i,b,j)  
#
           etemp = Taibj(a,i,b,j)*Vpiqj(a,i,b,j) 
           esumab += etemp 
#
     ENDPARDO a, b, i, j 

     execute sip_barrier 

     collective ecorraa += esuma
     execute print_scalar ecorraa 
#
     collective ecorrbb += esumb 
     execute print_scalar ecorrbb 
#
     collective ecorrab += esumab
     execute print_scalar ecorrab 
#
     ecorrT  = ecorraa + ecorrbb
     ecorrT += ecorrab 
     execute print_scalar ecorrT  
# 
#    ------------------------------------------------------------------------ 
# 
     ENDPROC ENERGY_NEW 
