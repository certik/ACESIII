      PROC FMEA
#     ----------
#
      PARDO i, a 
            Tme_a(i,a) = 0.0
            DO i1 
            DO a1 
               REQUEST VSpipi(a,i,a1,i1) i1  
               Tia(i,a)    = VSpipi(a,i,a1,i1)*Lt1aR(a1,i1) 
               Tme_a(i,a) += Tia(i,a) 
            ENDDO a1 
            ENDDO i1 
# 
            DO j 
            DO b 
               REQUEST Vpiqj(a,i,b,j) j  
               Tia(i,a)    = Vpiqj(a,i,b,j)*Lt1bR(b,j) 
               Tme_a(i,a) += Tia(i,a) 
            ENDDO b 
            ENDDO j 
#
            PUT Fme_a(i,a) = Tme_a(i,a) 
#
      ENDPARDO i, a 
# 
      ENDPROC FMEA
#     -------------
#
#    ------------------------------------------------------------------------ 
#
#    ------------------------------------------------------------------------ 
#
      PROC FMEB
#     ----------
#
      PARDO j, b 
            Tme_b(j,b) = 0.0
            DO j1 
            DO b1 
               REQUEST       VSqjqj(b,j,b1,j1) j1  
               Tjb(j,b)    = VSqjqj(b,j,b1,j1)*Lt1bR(b1,j1) 
               Tme_b(j,b) += Tjb(j,b) 
            ENDDO b1 
            ENDDO j1 
# 
            DO i 
            DO a 
               REQUEST       Vpiqj(a,i,b,j) j  
               Tjb(j,b)    = Vpiqj(a,i,b,j)*Lt1aR(a,i) 
               Tme_b(j,b) += Tjb(j,b) 
            ENDDO a 
            ENDDO i 
#
            PUT Fme_b(j,b) = Tme_b(j,b) 
#
      ENDPARDO j, b 
# 
      ENDPROC FMEB
#     -------------
#
#    ------------------------------------------------------------------------ 
#
#    ------------------------------------------------------------------------ 
#
      PROC FME
#     ----------
         CALL FMEA
         CALL FMEB
      ENDPROC FME
