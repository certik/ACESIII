#    ------------------------------------------------------------------------ 
#
      PROC FAEA
#     ----------
#
      PARDO a, a2, i
               allocate LWaiai(a2,i,a,*)
               DO a1
                  REQUEST            VSaaai(a1,a,a2,i) i
                  Taa(a,a1)        = VSaaai(a1,a,a2,i)*Lt1aR(a2,i)
                  LTae_a(a,a1) += Taa(a,a1)
                  DO i1
                     taiai(a2,i,a,i1)   = VSaaai(a1,a,a2,i)*Lt1aR(a1,i1)
                     LWaiai(a2,i,a,i1) += taiai(a2,i,a,i1)
                  ENDDO i1
               ENDDO a1
#
               DO i1
                  Taiai(a2,i,a,i1)          = LWaiai(a2,i,a,i1)
                  PREPARE e5aiai(a2,i,a,i1) = Taiai(a2,i,a,i1)
               ENDDO i1
#
               deallocate LWaiai(a2,i,a,*)
#
      ENDPARDO a, a2, i
#
      PARDO a, b, j
            allocate LWaibj(a,*,b,j)
            DO a1
               REQUEST               Vaabj(a1,a,b,j) j
               DO i
                  taibj(a,i,b,j)   = Vaabj(a1,a,b,j)*Lt1aR(a1,i)
                  LWaibj(a,i,b,j) += taibj(a,i,b,j)
               ENDDO i
               Taa(a,a1)     = Vaabj(a1,a,b,j)*Lt1bR(b,j)
               LTae_a(a,a1) += Taa(a,a1)
            ENDDO a1
#
            DO i
               Taibj(a,i,b,j)          = LWaibj(a,i,b,j)
               PREPARE e6aibj(a,i,b,j) = Taibj(a,i,b,j)
            ENDDO i
#
            deallocate LWaibj(a,*,b,j)
#
      ENDPARDO a, b, j
#
      PARDO a1, i, b, j
           REQUEST Vpiqj(a1,i,b,j) j
           DO a
              REQUEST         Taup_ab(a,i,b,j) j
              Taa(a,a1)     = Taup_ab(a,i,b,j)*Vpiqj(a1,i,b,j)
              Taa(a,a1)    *= -1.0
              LTae_a(a,a1) += Taa(a,a1)
           ENDDO a
      ENDPARDO a1, i, b, j
#
      PARDO a1, i, a2, i1
           REQUEST             VSpipi(a1,i1,a2,i) i
           DO a
               REQUEST         Taup_aa(a,i,a2,i1) i
               Taa(a,a1)     = Taup_aa(a,i,a2,i1)*VSpipi(a1,i1,a2,i)
               Taa(a,a1)    *= 0.5
               LTae_a(a,a1) += Taa(a,a1)
            ENDDO a
#
      ENDPARDO a1, i, a2, i1
#
      ENDPROC FAEA
#     -------------
#
#    ------------------------------------------------------------------------ 
#
#    ------------------------------------------------------------------------ 
#
      PROC FAEB
#     ----------
#
      PARDO b, b2, j
               allocate LWbjbj(b2,j,b,*) 
               DO b1 
                  REQUEST                 VSbbbj(b1,b,b2,j) j  
                  DO j1 
                     tbjbj(b2,j,b,j1)   = VSbbbj(b1,b,b2,j)*Lt1bR(b1,j1) 
                     LWbjbj(b2,j,b,j1) += tbjbj(b2,j,b,j1) 
                  ENDDO j1 
                  Tbb(b,b1)     = VSbbbj(b1,b,b2,j)*Lt1bR(b2,j)  
                  LTae_b(b,b1) += Tbb(b,b1) 
               ENDDO b1 
#
               DO j1 
                  Tbjbj(b2,j,b,j1)          = LWbjbj(b2,j,b,j1)
                  PREPARE e5bjbj(b2,j,b,j1) = Tbjbj(b2,j,b,j1) 
               ENDDO j1 
#
               deallocate LWbjbj(b2,j,b,*) 
#
      ENDPARDO b, b2, j
#
      PARDO a, i, b   
            allocate LWaibj(a,i,b,*) 
            DO b1  
               REQUEST               Vbbai(b1,b,a,i) i  
               DO j 
                  taibj(a,i,b,j)   = Vbbai(b1,b,a,i)*Lt1bR(b1,j) 
                  LWaibj(a,i,b,j) += taibj(a,i,b,j) 
               ENDDO j 
#
               Tbb(b,b1)     = Vbbai(b1,b,a,i)*Lt1aR(a,i) 
               LTae_b(b,b1) += Tbb(b,b1) 
#
            ENDDO b1  
#
            DO j 
               Taibj(a,i,b,j)          = LWaibj(a,i,b,j)
               PREPARE e5aibj(a,i,b,j) = Taibj(a,i,b,j) 
            ENDDO j 
#
            deallocate LWaibj(a,i,b,*) 
#
      ENDPARDO a, i, b   
#
      PARDO b1, j, a, i  
          REQUEST Vpiqj(a,i,b1,j) j  
          DO b  
             REQUEST         Taup_ab(a,i,b,j) j 
             Tbb(b,b1)     = Taup_ab(a,i,b,j)*Vpiqj(a,i,b1,j) 
             Tbb(b,b1)    *= -1.0  
             LTae_b(b,b1) += Tbb(b,b1) 
          ENDDO b  
      ENDPARDO b1, j, a, i  
#
      PARDO b1, j, b2, j1  
          REQUEST            VSqjqj(b1,j1,b2,j) j  
          DO b  
             REQUEST         Taup_bb(b,j,b2,j1) j  
             Tbb(b,b1)     = Taup_bb(b,j,b2,j1)*VSqjqj(b1,j1,b2,j) 
             Tbb(b,b1)    *= 0.5 
             LTae_b(b,b1) += Tbb(b,b1) 
          ENDDO b 
      ENDPARDO b1, j, b2, j1  
#
      ENDPROC FAEB
#     -------------
#
#    ------------------------------------------------------------------------ 
#
#    ------------------------------------------------------------------------ 
#
      PROC FAE
#     --------
         CALL FAEA 
         CALL FAEB 
      ENDPROC FAE
