# PPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPP 
#
      PROC WMINJ 
#
           CALL WminjAA 
           CALL WminjAB 
           CALL WminjBB 
#
      ENDPROC WMINJ 
# 
# ----------------------------------------------------------------------------
#
# ----------------------------------------------------------------------------
#
# In PROCEDURE T2NEWAA the new amplitudes T2new_aa are computed. Contributions 
# involving large two-particle intermediates are computed in the corresponding  
# procedures where these intermediates(temp) are computed. 
#
# Contributions coming from P_(ij)P_(ab)t^e_i t^a_m <mb||ej> are computed 
# elsewhere. 
#
# ----------------------------------------------------------------------------
#
      PROC T2NEWAA    
#     ------------
#
      PARDO a, i, i1
#
#           allocate arrays containing two-particle contracted arrays 
#           --------------------------------------------------------- 
            allocate Liiai(*,i,a,i1) 
            allocate Laiai(a,i,*,i1) 
#
#           fill in allocated arrays 
#           ------------------------ 
            DO i2 
# 
               REQUEST VSpipi(i2,i,a,i1) i1 
               Liiai(i2,i,a,i1) = VSpipi(i2,i,a,i1) 
#
            ENDDO i2 
#
            DO a2 
#
               REQUEST T2old_aa(a,i,a2,i1) i1
               Laiai(a,i,a2,i1) = T2old_aa(a,i,a2,i1) 
#
            ENDDO a2 
#
#           Start computation 
#           ----------------- 
            DO a1 
#
               REQUEST VSpipi(a,i,a1,i1) i1
#
               Taiai(a,i,a1,i1)   = VSpipi(a,i,a1,i1)
               TYaiai(a1,i,a,i1)  = 0.0
#
               DO i2
#
                 #GET t1a_old(a1,i2)
#
                  T1aiai(a1,i,a,i1) = Liiai(i2,i,a,i1)*Lt1aR(a1,i2)
                  T2aiai(a,i,a1,i1) = T1aiai(a1,i,a,i1)
#
                  Taiai(a,i,a1,i1)  += T2aiai(a,i,a1,i1)
                  TYaiai(a1,i,a,i1) -= T1aiai(a1,i,a,i1)
#
               ENDDO i2
#
               DO a2
#
                  #GET Fae_a(a1,a2)
                   tmp_aa(a1,a2) = LFae_a(a1,a2)
#
                   DO i2
#
                     #GET t1a_old(a1,i2)
                     #GET Fme_a(i2,a2)
                      tmp1_aa(a1,a2)  = Lt1aR(a1,i2)*LFme_a(i2,a2)
                      tmp1_aa(a1,a2) *= -0.5
                      tmp_aa(a1,a2)  += tmp1_aa(a1,a2)
#
                   ENDDO i2
#
                   T1aiai(a,i,a1,i1) = Laiai(a,i,a2,i1)*tmp_aa(a1,a2)
                   T2aiai(a1,i,a,i1) = T1aiai(a,i,a1,i1)
                   Taiai(a,i,a1,i1) += T1aiai(a,i,a1,i1)
                   TYaiai(a1,i,a,i1)-= T2aiai(a1,i,a,i1)
#
               ENDDO a2
#
               PREPARE T2NEW_aa(a,i,a1,i1) += Taiai(a,i,a1,i1)
               PREPARE T2NEW_aa(a1,i,a,i1) += TYaiai(a1,i,a,i1)
#
            ENDDO a1 
#
#           remove allocated arrays 
#           ----------------------- 
            deallocate Liiai(*,i,a,i1) 
            deallocate Laiai(a,i,*,i1) 
#
      ENDPARDO a, i, i1
#
      PARDO a, i, a1 
#
#           allocate arrays containing two-particle contracted arrays 
#           --------------------------------------------------------- 
            allocate L1aiai(a,i,a1,*) 
            allocate Laaai(a,*,a1,i) 
#
#           fill in allocated arrays 
#           ------------------------ 
            DO i2 
#
               REQUEST T2old_aa(a,i,a1,i2) i2 
               L1aiai(a,i,a1,i2) = T2old_aa(a,i,a1,i2) 
#
            ENDDO i2 
#
            DO a2
#
               REQUEST VSaaai(a,a2,a1,i) i
               Laaai(a,a2,a1,i) = VSaaai(a,a2,a1,i) 
#
            ENDDO a2
#
#           Start computation 
#           ----------------- 
            DO i1 
#
               Taiai(a,i,a1,i1)   = 0.0  
               TY1aiai(a,i1,a1,i) = 0.0
#
               DO i2
#
                 #GET Fmi_a(i2,i1)
                  tmp_ii(i2,i1) = LFmi_a(i2,i1)
#
                  DO a2
#
                    #GET t1a_old(a2,i1)
                    #GET Fme_a(i2,a2)
                     tmp1_ii(i2,i1) = Lt1aR(a2,i1)*LFme_a(i2,a2)
                     tmp1_ii(i2,i1) *= 0.5
                     tmp_ii(i2,i1)  += tmp1_ii(i2,i1)
#
                  ENDDO a2
#
                  T1aiai(a,i,a1,i1)  = L1aiai(a,i,a1,i2)*tmp_ii(i2,i1)
                  T2aiai(a,i1,a1,i)  = T1aiai(a,i,a1,i1)
                  Taiai(a,i,a1,i1)  -= T1aiai(a,i,a1,i1)
                  TY1aiai(a,i1,a1,i)+= T2aiai(a,i1,a1,i)
#
               ENDDO i2
#
               DO a2
#
                 #GET t1a_old(a2,i1)
#
                  T1aiai(a,i1,a1,i) = Laaai(a,a2,a1,i)*Lt1aR(a2,i1)
                  T2aiai(a,i,a1,i1) = T1aiai(a,i1,a1,i)
#
                  Taiai(a,i,a1,i1)  -= T2aiai(a,i,a1,i1)
                  TY1aiai(a,i1,a1,i)+= T1aiai(a,i1,a1,i)
#
               ENDDO a2
#
               PREPARE T2NEW_aa(a,i,a1,i1) += Taiai(a,i,a1,i1)
               PREPARE T2NEW_aa(a,i1,a1,i) += TY1aiai(a,i1,a1,i)
#
            ENDDO i1 
#
#           remove allocated arrays 
#           ----------------------- 
            deallocate L1aiai(a,i,a1,*) 
            deallocate Laaai(a,*,a1,i) 
#
      ENDPARDO a, i, a1 
#
      ENDPROC T2NEWAA    
#     ---------------
#
# PPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPP 
#
# PPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPP 
#
# In PROCEDURE T2NEWBB the new amplitudes T2new_bb are computed. Contributions 
# involving large two-particle intermediates are computed in the corresponding  
# procedures where these intermediates(temp) are computed. Thus the 
# intermediate Wminj is stored and its contribution computed here.  
#
# Contributions coming from P_(ij)P_(ab)t^e_i t^a_m <mb||ej> are computed 
# elsewhere. 
#
# The new amplitudes T2new_bb are FIRST updated in this procedure and 
# therefore created ate the begining of the procedure. 
#
      PROC T2NEWBB    
#     ------------
#
      PARDO b, j, j1
#
#           allocate arrays containing two-particle contracted arrays 
#           --------------------------------------------------------- 
            allocate Ljjbj(*,j,b,j1) 
            allocate Lbjbj(b,j,*,j1) 
#
#           fill in allocated arrays 
#           ------------------------ 
            DO j2 
# 
               REQUEST VSqjqj(j2,j,b,j1) j1 
               Ljjbj(j2,j,b,j1) = VSqjqj(j2,j,b,j1) 
#
            ENDDO j2 
#
            DO b2 
#
               REQUEST T2old_bb(b,j,b2,j1) j1
               Lbjbj(b,j,b2,j1) = T2old_bb(b,j,b2,j1) 
#
            ENDDO b2 
#
#           Start computation 
#           ----------------- 
            DO b1 
#
               REQUEST VSqjqj(b,j,b1,j1) j1
#
               Tbjbj(b,j,b1,j1)   = VSqjqj(b,j,b1,j1)
               TYbjbj(b1,j,b,j1)  = 0.0
#
               DO j2
#
                 #GET t1b_old(b1,j2)
#
                  T1bjbj(b1,j,b,j1) = Ljjbj(j2,j,b,j1)*Lt1bR(b1,j2)
                  T2bjbj(b,j,b1,j1) = T1bjbj(b1,j,b,j1)
#
                  Tbjbj(b,j,b1,j1)  += T2bjbj(b,j,b1,j1)
                  TYbjbj(b1,j,b,j1) -= T1bjbj(b1,j,b,j1)
#
               ENDDO j2
#
               DO b2
#
                  #GET Fae_b(b1,b2)
                   tmp_bb(b1,b2) = LFae_b(b1,b2)
#
                   DO j2
#
                     #GET t1b_old(b1,j2)
                     #GET Fme_b(j2,b2)
                      tmp1_bb(b1,b2)  = Lt1bR(b1,j2)*LFme_b(j2,b2)
                      tmp1_bb(b1,b2) *= -0.5
                      tmp_bb(b1,b2)  += tmp1_bb(b1,b2)
#
                   ENDDO j2
#
                   T1bjbj(b,j,b1,j1) = Lbjbj(b,j,b2,j1)*tmp_bb(b1,b2)
                   T2bjbj(b1,j,b,j1) = T1bjbj(b,j,b1,j1)
                   Tbjbj(b,j,b1,j1) += T1bjbj(b,j,b1,j1)
                   TYbjbj(b1,j,b,j1)-= T2bjbj(b1,j,b,j1)
#
               ENDDO b2
#
               PREPARE T2NEW_bb(b,j,b1,j1) += Tbjbj(b,j,b1,j1)
               PREPARE T2NEW_bb(b1,j,b,j1) += TYbjbj(b1,j,b,j1)
#
            ENDDO b1 
#
#           remove allocated arrays 
#           ----------------------- 
            deallocate Ljjbj(*,j,b,j1) 
            deallocate Lbjbj(b,j,*,j1) 
#
      ENDPARDO b, j, j1
#
      PARDO b, j, b1 
#
#           allocate arrays containing two-particle contracted arrays 
#           --------------------------------------------------------- 
            allocate L1bjbj(b,j,b1,*) 
            allocate Lbbbj(b,*,b1,j) 
#
#           fill in allocated arrays 
#           ------------------------ 
            DO j2 
#
               REQUEST T2old_bb(b,j,b1,j2) j2 
               L1bjbj(b,j,b1,j2) = T2old_bb(b,j,b1,j2) 
#
            ENDDO j2 
#
            DO b2
#
               REQUEST VSbbbj(b,b2,b1,j) j
               Lbbbj(b,b2,b1,j) = VSbbbj(b,b2,b1,j) 
#
            ENDDO b2
#
#           Start computation 
#           ----------------- 
            DO j1 
#
               Tbjbj(b,j,b1,j1)   = 0.0  
               TY1bjbj(b,j1,b1,j) = 0.0
#
               DO j2
#
                 #GET Fmi_b(j2,j1)
                  tmp_jj(j2,j1) = LFmi_b(j2,j1)
#
                  DO b2
#
                    #GET t1b_old(b2,j1)
                    #GET Fme_b(j2,b2)
                     tmp1_jj(j2,j1) = Lt1bR(b2,j1)*LFme_b(j2,b2)
                     tmp1_jj(j2,j1) *= 0.5
                     tmp_jj(j2,j1)  += tmp1_jj(j2,j1)
#
                  ENDDO b2
#
                  T1bjbj(b,j,b1,j1)  = L1bjbj(b,j,b1,j2)*tmp_jj(j2,j1)
                  T2bjbj(b,j1,b1,j)  = T1bjbj(b,j,b1,j1)
                  Tbjbj(b,j,b1,j1)  -= T1bjbj(b,j,b1,j1)
                  TY1bjbj(b,j1,b1,j)+= T2bjbj(b,j1,b1,j)
#
               ENDDO j2
#
               DO b2
#
                 #GET t1b_old(b2,j1)
#
                  T1bjbj(b,j1,b1,j) = Lbbbj(b,b2,b1,j)*Lt1bR(b2,j1)
                  T2bjbj(b,j,b1,j1) = T1bjbj(b,j1,b1,j)
#
                  Tbjbj(b,j,b1,j1)  -= T2bjbj(b,j,b1,j1)
                  TY1bjbj(b,j1,b1,j)+= T1bjbj(b,j1,b1,j)
#
               ENDDO b2
#
               PREPARE T2NEW_bb(b,j,b1,j1) += Tbjbj(b,j,b1,j1)
               PREPARE T2NEW_bb(b,j1,b1,j) += TY1bjbj(b,j1,b1,j)
#
            ENDDO j1 
#
#           remove allocated arrays 
#           ----------------------- 
            deallocate L1bjbj(b,j,b1,*) 
            deallocate Lbbbj(b,*,b1,j) 
#
      ENDPARDO b, j, b1 
#
      ENDPROC T2NEWBB    
#     ---------------
#
# PPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPP 
#
# PPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPP 
#
# In PROCEDURE T2NEWAB the new amplitudes T2new_ab are computed. Contributions 
# involving large two-particle intermediates are computed in the corresponding  
# procedures where these intermediates(temp) are computed. Thus the 
# intermediate Wminj is stored and its contribution computed here.  
#
# Contributions coming from P_(ij)P_(ab)t^e_i t^a_m <mb||ej> are computed 
# elsewhere. 
#
      PROC T2NEWAB    
#     ------------
#
      PARDO a, i, j 
#
            allocate         Laijj(a,i,*,j) 
            allocate         Laibj(a,i,*,j) 
#
            DO j1 
#
               REQUEST Vpiqj(a,i,j1,j) j 
               Laijj(a,i,j1,j) = Vpiqj(a,i,j1,j) 
#
            ENDDO j1 
#
            DO b1 
#
               REQUEST T2old_ab(a,i,b1,j) j 
               Laibj(a,i,b1,j) = T2old_ab(a,i,b1,j) 
#
            ENDDO b1 
#
            DO b 
#
               REQUEST          Vpiqj(a,i,b,j) j 
               Taibj(a,i,b,j) = Vpiqj(a,i,b,j) 
#
               DO j1 
#
                 #GET t1b_old(b,j1) 
#
                  T1aibj(a,i,b,j) = Laijj(a,i,j1,j)*Lt1bR(b,j1) 
                  Taibj(a,i,b,j) -= T1aibj(a,i,b,j) 
#
               ENDDO j1 
#
               DO b1 
#
                 #GET Fae_b(b,b1) 
                  tmp_bb(b,b1) = LFae_b(b,b1)    
#
                  DO j1 
#
                    #GET t1b_old(b,j1) 
                    #GET Fme_b(j1,b1) 
#
                     tmp1_bb(b,b1)  = Lt1bR(b,j1)*LFme_b(j1,b1) 
                     tmp1_bb(b,b1) *= 0.5 
                     tmp_bb(b,b1)  -= tmp1_bb(b,b1)  
#
                  ENDDO j1 
#
                  T1aibj(a,i,b,j) = Laibj(a,i,b1,j)*tmp_bb(b,b1)  
                  Taibj(a,i,b,j) += T1aibj(a,i,b,j) 
#
               ENDDO b1 
#
               PREPARE T2NEW_AB(a,i,b,j) += Taibj(a,i,b,j)  
#
            ENDDO b 
#
            deallocate Laijj(a,i,*,j) 
            deallocate Laibj(a,i,*,j) 
#
      ENDPARDO a, i, j 
#
      PARDO i, b, j 
#
            allocate Liibj(*,i,b,j) 
            allocate L1aibj(*,i,b,j)   
#
            DO i1 
#
               REQUEST Vpiqj(i1,i,b,j) j 
               Liibj(i1,i,b,j) = Vpiqj(i1,i,b,j) 
#
            ENDDO i1 
#
            DO a1 
#
               REQUEST T2old_ab(a1,i,b,j) j 
               L1aibj(a1,i,b,j) = T2old_ab(a1,i,b,j) 
#
            ENDDO a1 
#
            DO a 
#
               Taibj(a,i,b,j) = 0.0  
#
               DO i1 
#
                 #GET t1a_old(a,i1) 
#
                  T1aibj(a,i,b,j) = Liibj(i1,i,b,j)*Lt1aR(a,i1) 
                  Taibj(a,i,b,j) -= T1aibj(a,i,b,j) 
#
               ENDDO i1 
#
               DO a1 
#
                 #GET Fae_a(a,a1) 
                  tmp_aa(a,a1) = LFae_a(a,a1)    
#
                  DO i1 
#
                    #GET t1a_old(a,i1) 
                    #GET Fme_a(i1,a1) 
                     tmp1_aa(a,a1)  = Lt1aR(a,i1)*LFme_a(i1,a1) 
                     tmp1_aa(a,a1) *= 0.5 
                     tmp_aa(a,a1)  -= tmp1_aa(a,a1)  
#
                  ENDDO i1 
#
                  T1aibj(a,i,b,j) = L1aibj(a1,i,b,j)*tmp_aa(a,a1)  
                  Taibj(a,i,b,j) += T1aibj(a,i,b,j) 
#
               ENDDO a1 
#
               PREPARE T2NEW_AB(a,i,b,j) += Taibj(a,i,b,j)  
#
            ENDDO a 
#
            deallocate Liibj(*,i,b,j) 
            deallocate L1aibj(*,i,b,j)   
#
      ENDPARDO i, b, j 
#
      PARDO a, i, b 
#
            allocate Lbbai(b,*,a,i) 
            allocate L2aibj(a,i,b,*) 
#
            DO b1 
#
               REQUEST Vbbai(b,b1,a,i) i 
               Lbbai(b,b1,a,i) = Vbbai(b,b1,a,i) 
#
            ENDDO b1 
#
            DO j2  
#
               REQUEST T2old_ab(a,i,b,j2) j2  
               L2aibj(a,i,b,j2) = T2old_ab(a,i,b,j2)  
#
            ENDDO j2  
#
            DO j 
#
               Taibj(a,i,b,j) = 0.0  
#
               DO b1 
#
                 #GET t1b_old(b1,j) 
#
                  T1aibj(a,i,b,j) = Lbbai(b,b1,a,i)*Lt1bR(b1,j)  
                  Taibj(a,i,b,j) += T1aibj(a,i,b,j) 
#
               ENDDO b1 
#
               DO j2 
#
                 #GET Fmi_b(j2,j) 
                  tmp_jj(j2,j) = LFmi_b(j2,j)  
#
                  DO b2 
#
                    #GET t1b_old(b2,j) 
                    #GET Fme_b(j2,b2) 
                     tmp1_jj(j2,j)  = Lt1bR(b2,j)*LFme_b(j2,b2) 
                     tmp1_jj(j2,j) *= 0.5 
                     tmp_jj(j2,j)  += tmp1_jj(j2,j) 
#
                  ENDDO b2 
#
                  T1aibj(a,i,b,j) = L2aibj(a,i,b,j2)*tmp_jj(j2,j)  
                  Taibj(a,i,b,j) -= T1aibj(a,i,b,j) 
#
               ENDDO j2 
#
               PREPARE T2NEW_AB(a,i,b,j) += Taibj(a,i,b,j)  
#
            ENDDO j 
#
            deallocate Lbbai(b,*,a,i) 
            deallocate L2aibj(a,i,b,*) 
#
      ENDPARDO a, i, b  
#
      PARDO a, b, j 
#
            allocate L3aibj(a,*,b,j) 
            allocate Laabj(a,*,b,j) 
#
            DO i1 
#
               REQUEST T2old_ab(a,i1,b,j) j 
               L3aibj(a,i1,b,j) = T2old_ab(a,i1,b,j) 
#
            ENDDO i1 
#
            DO a1 
#
               REQUEST Vaabj(a,a1,b,j) j  
               Laabj(a,a1,b,j) = Vaabj(a,a1,b,j)  
#
            ENDDO a1 
#
            DO i 
#
               Taibj(a,i,b,j) = 0.0  
#
               DO i1 
#
                 #GET Fmi_a(i1,i) 
                  tmp_ii(i1,i) = LFmi_a(i1,i) 
#
                  DO a1 
#
                    #GET t1a_old(a1,i) 
                    #GET Fme_a(i1,a1) 
#
                     tmp1_ii(i1,i)  = Lt1aR(a1,i)*LFme_a(i1,a1) 
                     tmp1_ii(i1,i) *= 0.5 
                     tmp_ii(i1,i)  += tmp1_ii(i1,i) 
#
                  ENDDO a1  
#
                  T1aibj(a,i,b,j) = L3aibj(a,i1,b,j)*tmp_ii(i1,i) 
                  Taibj(a,i,b,j) -= T1aibj(a,i,b,j) 
#
               ENDDO i1 
#
               DO a1 
#
                 #GET t1a_old(a1,i) 
#
                  T1aibj(a,i,b,j) = Laabj(a,a1,b,j)*Lt1aR(a1,i)  
                  Taibj(a,i,b,j) += T1aibj(a,i,b,j) 
#
               ENDDO a1 
#
               PREPARE T2NEW_AB(a,i,b,j) += Taibj(a,i,b,j)  
#
            ENDDO i 
#
            deallocate L3aibj(a,*,b,j) 
            deallocate Laabj(a,*,b,j) 
#
      ENDPARDO a, b, j 
#
      ENDPROC T2NEWAB    
#     ---------------
#
# PPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPP 
#
#    ------------------------------------------------------------------------ 
#
     PROC AOLADDER  
#
#    ------------------------------------------------------------------------ 
#
#    Form Half back transformed cluster arrays  
#    -----------------------------------------
#
     PARDO a1, i1, i   
#
           allocate LLaiai(*,i,a1,i1) 
#
           DO a  
#
              REQUEST Tau_aa(a, i,a1,i1) i1 
             #REQUEST Tau_aa(a,i1,a1,i)  i1 
              LLaiai(a,i,a1,i1)  = Tau_aa(a, i,a1,i1) 
              LLaiai(a,i,a1,i1) *= 2.0  
             #Taiai(a,i,a1,i1)   = Tau_aa(a,i1,a1,i) 
             #LLaiai(a,i,a1,i1) -= Taiai(a,i,a1,i1)  
#
           ENDDO a  
#
           DO lambda   
#
              Zaa(lambda,i,a1,i1) = 0.0 
#
              DO a  
#
                 Txiai(lambda,i,a1,i1) = LLaiai(a,i,a1,i1)*ca(lambda,a)
                 Zaa(lambda,i,a1,i1)  += Txiai(lambda,i,a1,i1) 
#
              ENDDO a 
#
              PREPARE T1AO_aa(lambda,i,a1,i1) = Zaa(lambda,i,a1,i1) 
#
           ENDDO lambda  
#
           deallocate LLaiai(*,i,a1,i1) 
#
     ENDPARDO a1, i1, i   
#
     PARDO b1, j1, j   
#
           allocate LLbjbj(*,j,b1,j1) 
#
           DO b  
#
              REQUEST Tau_bb(b, j,b1,j1) j1 
             #REQUEST Tau_bb(b,j1,b1,j)  j1 
              LLbjbj(b,j,b1,j1)  = Tau_bb(b, j,b1,j1) 
              LLbjbj(b,j,b1,j1) *= 2.0  
             #Tbjbj(b,j,b1,j1)   = Tau_bb(b,j1,b1,j) 
             #LLbjbj(b,j,b1,j1) -= Tbjbj(b,j,b1,j1)  
#
           ENDDO b  
#
           DO lambda   
#
              Zbb(lambda,j,b1,j1) = 0.0 
#
              DO b  
#
                 Txjbj(lambda,j,b1,j1) = LLbjbj(b,j,b1,j1)*cb(lambda,b)
                 Zbb(lambda,j,b1,j1)  += Txjbj(lambda,j,b1,j1) 
#
              ENDDO b 
#
              PREPARE T1AO_bb(lambda,j,b1,j1) = Zbb(lambda,j,b1,j1) 
#
           ENDDO lambda  
#
           deallocate LLbjbj(*,j,b1,j1) 
#
     ENDPARDO b1, j1, j   
#
     PARDO b, j, i   
#
           allocate LLaibj(*,i,b,j) 
#
           DO a  
#
              REQUEST Tau_ab(a,i,b,j) j 
              LLaibj(a,i,b,j) = Tau_ab(a,i,b,j) 
#
           ENDDO a  
#
           DO lambda   
#
              Zab(lambda,i,b,j) = 0.0 
#
              DO a  
#
                 Txibj(lambda,i,b,j) = LLaibj(a,i,b,j)*ca(lambda,a)
                 Zab(lambda,i,b,j)  += Txibj(lambda,i,b,j) 
#
              ENDDO a 
#
              PREPARE T1AO_ab(lambda,i,b,j) = Zab(lambda,i,b,j) 
#
           ENDDO lambda  
#
           deallocate LLaibj(*,i,b,j) 
#
     ENDPARDO b, j, i   
     execute server_barrier 
#
     PARDO lambda, i, i1  
#
           allocate LAO_aa(lambda,i,*,i1) 
#
           DO a1 
#
              REQUEST T1AO_aa(lambda,i,a1,i1) i1  
#
              DO sigma 
#
                 Txixi(lambda,i,sigma,i1)   = T1AO_aa(lambda,i,a1,i1)*ca(sigma,a1)
                 LAO_aa(lambda,i,sigma,i1) += Txixi(lambda,i,sigma,i1)  
#
              ENDDO sigma  
#
           ENDDO a1  
#
           DO sigma 
#
              Txixi(lambda,i,sigma,i1)           = LAO_aa(lambda,i,sigma,i1) 
              PREPARE TAO_aa(lambda,i,sigma,i1)  = Txixi(lambda,i,sigma,i1)
             #ZZaa(lambda,i,sigma,i1)            = 0.0 
             #PREPARE T2AO_aa(lambda,i,sigma,i1) = ZZaa(lambda,i,sigma,i1) 
#
           ENDDO sigma 
#
           deallocate LAO_aa(lambda,i,*,i1) 
#
     ENDPARDO lambda, i, i1   
#
     PARDO lambda, j, j1  
#
           allocate LAO_bb(lambda,j,*,j1) 
#
           DO b1 
#
              REQUEST T1AO_bb(lambda,j,b1,j1) j1  
#
              DO sigma 
#
                 Txjxj(lambda,j,sigma,j1)   = T1AO_bb(lambda,j,b1,j1)*cb(sigma,b1)
                 LAO_bb(lambda,j,sigma,j1) += Txjxj(lambda,j,sigma,j1)  
#
              ENDDO sigma  
#
           ENDDO b1  
#
           DO sigma 
#
              Txjxj(lambda,j,sigma,j1)           = LAO_bb(lambda,j,sigma,j1) 
              PREPARE TAO_bb(lambda,j,sigma,j1)  = Txjxj(lambda,j,sigma,j1)
             #ZZbb(lambda,j,sigma,j1)            = 0.0 
             #PREPARE T2AO_bb(lambda,j,sigma,j1) = ZZbb(lambda,j,sigma,j1) 
#
           ENDDO sigma 
#
           deallocate LAO_bb(lambda,j,*,j1) 
#
     ENDPARDO lambda, j, j1   
#
     PARDO lambda, i, j  
#
           allocate LAO_ab(lambda,i,*,j)  
#
           DO b 
#
              REQUEST T1AO_ab(lambda,i,b,j) j  
#
              DO sigma 
#
                 Txixj(lambda,i,sigma,j)   = T1AO_ab(lambda,i,b,j)*cb(sigma,b)
                 LAO_ab(lambda,i,sigma,j) += Txixj(lambda,i,sigma,j)
#
              ENDDO sigma 
#
           ENDDO b 
#
           DO sigma 
              Txixj(lambda,i,sigma,j)           = LAO_ab(lambda,i,sigma,j)  
              PREPARE TAO_ab(lambda,i,sigma,j)  = Txixj(lambda,i,sigma,j)
             #Yab(lambda,i,sigma,j)             = 0.0 
             #PREPARE T2AO_ab(lambda,i,sigma,j) = Yab(lambda,i,sigma,j) 
           ENDDO sigma 
#
           deallocate LAO_ab(lambda,i,*,j)  
#
     ENDPARDO lambda, i, j   
#
     PARDO lambda, sigma, i, i1  
           ZZaa(lambda,i,sigma,i1)            = 0.0 
           PREPARE T2AO_aa(lambda,i,sigma,i1) = ZZaa(lambda,i,sigma,i1) 
     ENDPARDO lambda, sigma, i, i1   
#
     PARDO lambda, sigma, j, j1  
           ZZbb(lambda,j,sigma,j1)            = 0.0 
           PREPARE T2AO_bb(lambda,j,sigma,j1) = ZZbb(lambda,j,sigma,j1) 
     ENDPARDO lambda, sigma, j, j1   
#
     PARDO lambda, sigma, i, j1  
           ZZab(lambda,i,sigma,j1)            = 0.0 
           PREPARE T2AO_ab(lambda,i,sigma,j1) = ZZab(lambda,i,sigma,j1) 
     ENDPARDO lambda, sigma, i, j1   
#
     create t1a_ax 
     create t1b_bx 
     execute server_barrier   
#
#    Contract AOINT with half back transformed Amplitudes 
#    ----------------------------------------------------    
#
     PARDO lambda, sigma, mu, nu   
#
           compute_integrals           aoint(lambda,mu,sigma,nu)
          #txxxx(lambda,mu,sigma,nu) = aoint(lambda,mu,sigma,nu)
#
           DO i
           DO j
              REQUEST                       TAO_ab(lambda,i,sigma,j) j
              Yab(mu,i,nu,j)              = aoint(lambda,mu,sigma,nu)*TAO_ab(lambda,i,sigma,j)
              PREPARE T2AO_ab(mu,i,nu,j) += Yab(mu,i,nu,j)
           ENDDO j
           ENDDO i
#             
           DO i
           DO i1
              if i < i1
                 REQUEST                        TAO_aa(lambda,i,sigma,i1) i1
                 Yaa(mu,i,nu,i1)              = aoint(lambda,mu,sigma,nu)*TAO_aa(lambda,i,sigma,i1)
                 Y1aa(nu,i1,mu,i)             = Yaa(mu,i,nu,i1)
                 PREPARE T2AO_aa(mu,i,nu,i1) += Yaa(mu,i,nu,i1)
                 PREPARE T2AO_aa(nu,i1,mu,i) += Y1aa(nu,i1,mu,i)
              endif
              if i == i1
                 REQUEST                        TAO_aa(lambda,i,sigma,i1) i1
                 Yaa(mu,i,nu,i1)              = aoint(lambda,mu,sigma,nu)*TAO_aa(lambda,i,sigma,i1)
                 PREPARE T2AO_aa(mu,i,nu,i1) += Yaa(mu,i,nu,i1)
              endif
           ENDDO i1 
           ENDDO i
#             
           DO j
           DO j1 
              if j < j1
                 REQUEST                        TAO_bb(lambda,j,sigma,j1) j1
                 Ybb(mu,j,nu,j1)              = aoint(lambda,mu,sigma,nu)*TAO_bb(lambda,j,sigma,j1)
                 Y1bb(nu,j1,mu,j)             = Ybb(mu,j,nu,j1)
                 PREPARE T2AO_bb(mu,j,nu,j1) += Ybb(mu,j,nu,j1)
                 PREPARE T2AO_bb(nu,j1,mu,j) += Y1bb(nu,j1,mu,j)
              endif
              if j == j1
                 REQUEST                        TAO_bb(lambda,j,sigma,j1) j1
                 Ybb(mu,j,nu,j1)              = aoint(lambda,mu,sigma,nu)*TAO_bb(lambda,j,sigma,j1)
                 PREPARE T2AO_bb(mu,j,nu,j1) += Ybb(mu,j,nu,j1)
              endif
           ENDDO j1
           ENDDO j
#
     ENDPARDO lambda, sigma, mu, nu   
#
#    Half transform the t1 arrays. 
#    ----------------------------- 
#
     PARDO a, mu
#
           tpx(a,mu) = 0.0 
#
           DO i  
#
             #GET          t1a_old(a,i) 
              t1px(a,mu) = Lt1aR(a,i)*ca(mu,i) 
              tpx(a,mu) += t1px(a,mu) 
#
           ENDDO i  
#
           PUT t1a_ax(a,mu) = tpx(a,mu) 
#
     ENDPARDO a, mu
#
     PARDO b, mu
#
           tqx(b,mu) = 0.0 
#
           DO j  
#
             #GET          t1b_old(b,j) 
              t1qx(b,mu) = Lt1bR(b,j)*cb(mu,j) 
              tqx(b,mu) += t1qx(b,mu) 
#
           ENDDO j  
#
           PUT t1b_bx(b,mu) = tqx(b,mu) 
#
     ENDPARDO b, mu
#
     execute server_barrier  
     execute sip_barrier  
#
#    Perform final transformation 
#    ---------------------------- 
#
     PARDO nu, i, i1
           allocate LLaa(*,i,nu,i1)
           DO mu
              REQUEST T2AO_aa(mu,i,nu,i1) i1
              DO a
                 Taixi(a,i,nu,i1) = T2AO_aa(mu,i,nu,i1)*ca(mu,a)
                 LLaa(a,i,nu,i1) += Taixi(a,i,nu,i1)
              ENDDO a
           ENDDO mu
           DO a
              LLaa(a,i,nu,i1) *= 0.5
              DO a1
                 Taiai(a,i,a1,i1)             = LLaa(a,i,nu,i1)*ca(nu,a1)
                 PREPARE T2new_aa(a,i,a1,i1) += Taiai(a,i,a1,i1)
              ENDDO a1
           ENDDO a
           DO a
              LLaa(a,i,nu,i1) *= -1.0
              DO a1
                 GET                            t1a_ax(a1,nu)
                 Taiai(a,i,a1,i1)             = LLaa(a,i,nu,i1)*t1a_ax(a1,nu)
                 PREPARE T2new_aa(a,i,a1,i1) += Taiai(a,i,a1,i1)
                 T3aiai(a1,i,a,i1)            = Taiai(a,i,a1,i1)
                 T3aiai(a1,i,a,i1)           *= -1.0
                 PREPARE T2new_aa(a1,i,a,i1) += T3aiai(a1,i,a,i1)
              ENDDO a1
           ENDDO a
           deallocate LLaa(*,i,nu,i1)
     ENDPARDO nu, i, i1
#
     PARDO nu, j, j1
           allocate LLbb(*,j,nu,j1)
           DO mu
              REQUEST T2AO_bb(mu,j,nu,j1) j1
              DO b
                 Tbjxj(b,j,nu,j1) = T2AO_bb(mu,j,nu,j1)*cb(mu,b)
                 LLbb(b,j,nu,j1) += Tbjxj(b,j,nu,j1)
              ENDDO b
           ENDDO mu
           DO b
              LLbb(b,j,nu,j1) *= 0.5
              DO b1
                 Tbjbj(b,j,b1,j1)             = LLbb(b,j,nu,j1)*cb(nu,b1)
                 PREPARE T2new_bb(b,j,b1,j1) += Tbjbj(b,j,b1,j1)
              ENDDO b1
           ENDDO b
           DO b
              LLbb(b,j,nu,j1) *= -1.0
              DO b1
                 GET                            t1b_bx(b1,nu)
                 Tbjbj(b,j,b1,j1)             = LLbb(b,j,nu,j1)*t1b_bx(b1,nu)
                 PREPARE T2new_bb(b,j,b1,j1) += Tbjbj(b,j,b1,j1)
                 T3bjbj(b1,j,b,j1)            = Tbjbj(b,j,b1,j1)
                 T3bjbj(b1,j,b,j1)           *= -1.0
                 PREPARE T2new_bb(b1,j,b,j1) += T3bjbj(b1,j,b,j1)
              ENDDO b1
           ENDDO b
           deallocate LLbb(*,j,nu,j1)
     ENDPARDO nu, j, j1
#
     PARDO nu, i, j
           allocate LLab(*,i,nu,j)
           DO mu
              REQUEST T2AO_ab(mu,i,nu,j) j
              DO a
                 Taixj(a,i,nu,j) = T2AO_ab(mu,i,nu,j)*ca(mu,a)
                 LLab(a,i,nu,j) += Taixj(a,i,nu,j)
              ENDDO a
           ENDDO mu
           DO a
              DO b
                 GET                          t1b_bx(b,nu)
                 Taibj(a,i,b,j)             = LLab(a,i,nu,j)*cb(nu,b)
                 PREPARE T2new_ab(a,i,b,j) += Taibj(a,i,b,j)
#
                 T1aibj(a,i,b,j)            = LLab(a,i,nu,j)*t1b_bx(b,nu)
                 T1aibj(a,i,b,j)           *= -1.0
                 PREPARE T2new_ab(a,i,b,j) += T1aibj(a,i,b,j)
              ENDDO b
           ENDDO a
           deallocate LLab(*,i,nu,j)
     ENDPARDO nu, i, j
#
     PARDO nu, i, j
           allocate LLab(*,i,nu,j)
           DO mu
              REQUEST T2AO_ab(mu,i,nu,j) j
              DO a
                 GET               t1a_ax(a,mu)
                 Taixj(a,i,nu,j) = T2AO_ab(mu,i,nu,j)*t1a_ax(a,mu)
                 LLab(a,i,nu,j) -= Taixj(a,i,nu,j)
              ENDDO a
           ENDDO mu
           DO a
              DO b
                 Taibj(a,i,b,j)             = LLab(a,i,nu,j)*cb(nu,b)
                 PREPARE T2new_ab(a,i,b,j) += Taibj(a,i,b,j)
              ENDDO b
           ENDDO a
           deallocate LLab(*,i,nu,j)
     ENDPARDO nu, i, j
#
     execute sip_barrier  
     delete t1a_ax 
     delete t1b_bx 
#
#    ------------------------------------------------------------------------ 
#
     ENDPROC AOLADDER  
#
#    ------------------------------------------------------------------------ 
#
#    ------------------------------------------------------------------------ 
#
# This SUPER procedure contains the procedures WAEBFAA, WAEBFBB, and 
# WAEBFAB which compute the contributions to these intermediates excluding  
# the terms containing the 4-virtual integrals. The intermediates are 
# computed as temporary arrays and used to update the amplitudes as they 
# are formed. The AOLADDER contribution is computed seperately.  
#
#    ------------------------------------------------------------------------ 
#
     PROC WAEBFAA 
#     ------------ 
#
# --------------------------------------------------------------------- 
# Comute contribution coming from:  
# 1/2 tau^{ab}_{mn} * [ 1/4 tau^{ef}_{ij} * (V^{mn}_{ef} - V^{mn}_{fe}]  
# --------------------------------------------------------------------- 
#
     create Wminj_aa 
     execute sip_barrier 
     PARDO i, i1, i2  
#
           DO i3 
#
              IF i < i3  
#
                 Tiiii(i1,i,i2,i3) = 0.0
#
                 DO a  
                 DO a1  
#
                    REQUEST Tau_aa(a,i,a1,i3) i3 
                    REQUEST VSpipi(a,i1,a1,i2) i2 
#
                    T1iiii(i1,i,i2,i3) = Tau_aa(a,i,a1,i3)*VSpipi(a,i1,a1,i2)
                    Tiiii(i1,i,i2,i3) += T1iiii(i1,i,i2,i3)
#
                 ENDDO a1  
                 ENDDO a   
#
                 T1iiii(i2,i3,i1,i)        = Tiiii(i1,i,i2,i3)
                 PUT Wminj_aa(i1,i,i2,i3) += Tiiii(i1,i,i2,i3) 
                 PUT Wminj_aa(i2,i3,i1,i) += T1iiii(i2,i3,i1,i) 
#
              ENDIF # i < i3  
#
              IF i == i3  
#
                 Tiiii(i1,i,i2,i3) = 0.0
#
                 DO a  
                 DO a1  
#
                    REQUEST Tau_aa(a,i,a1,i3) i3 
                    REQUEST VSpipi(a,i1,a1,i2) i2 
#
                    T1iiii(i1,i,i2,i3) = Tau_aa(a,i,a1,i3)*VSpipi(a,i1,a1,i2)
                    Tiiii(i1,i,i2,i3) += T1iiii(i1,i,i2,i3)
#
                 ENDDO a1  
                 ENDDO a   
#
                 PUT Wminj_aa(i1,i,i2,i3) += Tiiii(i1,i,i2,i3) 
#
              ENDIF # i == i3  
#
           ENDDO i3 
#
     ENDPARDO i, i1, i2  
     execute sip_barrier 
#
     PARDO a, i, i3 
#
           DO a1 
#
              IF a < a1 
#
                 R1aiai(a,i,a1,i3) = 0.0 
#
                 DO i1   
                 DO i2   
#
                    REQUEST Tau_aa(a,i1,a1,i2) i2 
                    GET Wminj_aa(i1,i,i2,i3) 
#
                    Taiai(a,i,a1,i3)   = Tau_aa(a,i1,a1,i2)*Wminj_aa(i1,i,i2,i3)
                    R1aiai(a,i,a1,i3) += Taiai(a,i,a1,i3)  
#
                 ENDDO i2   
                 ENDDO i1    
#
                 R1aiai(a,i,a1,i3)           *= 0.125
                 Taiai(a1,i3,a,i)             = R1aiai(a,i,a1,i3)  
                 PREPARE T2new_aa(a,i,a1,i3) += R1aiai(a,i,a1,i3)
                 PREPARE T2new_aa(a1,i3,a,i) += Taiai(a1,i3,a,i)
#
              ENDIF # a < a1 
#
              IF a == a1 
#
                 R1aiai(a,i,a1,i3) = 0.0 
#
                 DO i1   
                 DO i2   
#
                    REQUEST Tau_aa(a,i1,a1,i2) i2 
                    GET Wminj_aa(i1,i,i2,i3) 
#
                    Taiai(a,i,a1,i3)  = Tau_aa(a,i1,a1,i2)*Wminj_aa(i1,i,i2,i3)
                    R1aiai(a,i,a1,i3) += Taiai(a,i,a1,i3)  
#
                 ENDDO i2   
                 ENDDO i1    
#
                 R1aiai(a,i,a1,i3)       *= 0.125
                 PREPARE T2new_aa(a,i,a1,i3) += R1aiai(a,i,a1,i3)
#
              ENDIF # a == a1 
#
           ENDDO a1 
#
     ENDPARDO a,  i, i3 
     execute sip_barrier 
     delete Wminj_aa 
#
     ENDPROC WAEBFAA 
#    --------------- 
#
# --------------------------------------------------------------------------- 
#
#    ------------------------------------------------------------------------ 
#
     PROC WAEBFBB 
#    ------------- 
#
# --------------------------------------------------------------------- 
# Comute contribution coming from:  
# 1/2 tau^{ab}_{mn} * [ 1/4 tau^{ef}_{ij} * (V^{mn}_{ef} - V^{mn}_{fe}]  
# --------------------------------------------------------------------- 
#
     create Wminj_bb 
     execute sip_barrier 
     PARDO j, j1, j2  
#
           DO j3 
#
              IF j < j3  
#
                 Tjjjj(j1,j,j2,j3) = 0.0
#
                 DO b  
                 DO b1  
#
                    REQUEST Tau_bb(b,j,b1,j3) j3 
                    REQUEST VSqjqj(b,j1,b1,j2) j2 
#
                    T1jjjj(j1,j,j2,j3) = Tau_bb(b,j,b1,j3)*VSqjqj(b,j1,b1,j2)
                    Tjjjj(j1,j,j2,j3) += T1jjjj(j1,j,j2,j3)
#
                 ENDDO b1  
                 ENDDO b   
#
                 T1jjjj(j2,j3,j1,j)        = Tjjjj(j1,j,j2,j3)
                 PUT Wminj_bb(j1,j,j2,j3) += Tjjjj(j1,j,j2,j3) 
                 PUT Wminj_bb(j2,j3,j1,j) += T1jjjj(j2,j3,j1,j) 
#
              ENDIF # j < j3  
#
              IF j == j3  
#
                 Tjjjj(j1,j,j2,j3) = 0.0
#
                 DO b  
                 DO b1  
#
                    REQUEST Tau_bb(b,j,b1,j3) j3 
                    REQUEST VSqjqj(b,j1,b1,j2) j2 
#
                    T1jjjj(j1,j,j2,j3) = Tau_bb(b,j,b1,j3)*VSqjqj(b,j1,b1,j2)
                    Tjjjj(j1,j,j2,j3) += T1jjjj(j1,j,j2,j3)
#
                 ENDDO b1  
                 ENDDO b   
#
                 PUT Wminj_bb(j1,j,j2,j3) += Tjjjj(j1,j,j2,j3) 
#
              ENDIF # j == j3  
#
           ENDDO j3 
#
     ENDPARDO j, j1, j2  
     execute sip_barrier 
#
     PARDO b, j, j3 
#
           DO b1 
#
              IF b < b1 
#
                 R1bjbj(b,j,b1,j3) = 0.0 
#
                 DO j1   
                 DO j2   
#
                    REQUEST Tau_bb(b,j1,b1,j2) j2 
                    GET Wminj_bb(j1,j,j2,j3) 
#
                    Tbjbj(b,j,b1,j3)   = Tau_bb(b,j1,b1,j2)*Wminj_bb(j1,j,j2,j3)
                    R1bjbj(b,j,b1,j3) += Tbjbj(b,j,b1,j3)  
#
                 ENDDO j2   
                 ENDDO j1    
#
                 R1bjbj(b,j,b1,j3)           *= 0.125
                 Tbjbj(b1,j3,b,j)             = R1bjbj(b,j,b1,j3)  
                 PREPARE T2new_bb(b,j,b1,j3) += R1bjbj(b,j,b1,j3)
                 PREPARE T2new_bb(b1,j3,b,j) += Tbjbj(b1,j3,b,j)
#
              ENDIF # b < b1 
#
              IF b == b1 
#
                 R1bjbj(b,j,b1,j3) = 0.0 
#
                 DO j1   
                 DO j2   
#
                    REQUEST Tau_bb(b,j1,b1,j2) j2 
                    GET Wminj_bb(j1,j,j2,j3) 
#
                    Tbjbj(b,j,b1,j3)  = Tau_bb(b,j1,b1,j2)*Wminj_bb(j1,j,j2,j3)
                    R1bjbj(b,j,b1,j3) += Tbjbj(b,j,b1,j3)  
#
                 ENDDO j2   
                 ENDDO j1    
#
                 R1bjbj(b,j,b1,j3)       *= 0.125
                 PREPARE T2new_bb(b,j,b1,j3) += R1bjbj(b,j,b1,j3)
#
              ENDIF # b == b1 
#
           ENDDO b1 
#
     ENDPARDO b,  j, j3 
     execute sip_barrier 
     delete Wminj_bb 
#
     ENDPROC WAEBFBB 
#    --------------- 
#
# --------------------------------------------------------------------------- 
#
# --------------------------------------------------------------------------- 
#
      PROC WAEBFAB 
#     ------------ 
#
# --------------------------------------------------------------------- 
# Comute contribution coming from:  
# 1/2 tau^{ab}_{mn} * [ 1/4 tau^{ef}_{ij} * (V^{mn}_{ef} - V^{mn}_{fe}]  
# --------------------------------------------------------------------- 
#
      create Wminj_ab 
      execute sip_barrier 
      PARDO i, j, i1, j1 
#
            Tiijj(i1,i,j1,j) = 0.0 
#
            DO a  
            DO b 
#
               REQUEST Tau_ab(a,i,b,j)  j  
               REQUEST Vpiqj(a,i1,b,j1) j1  
#
               T1iijj(i1,i,j1,j) = Vpiqj(a,i1,b,j1)*Tau_ab(a,i,b,j) 
               Tiijj(i1,i,j1,j) += T1iijj(i1,i,j1,j)  
#
            ENDDO b 
            ENDDO a  
#
            PUT Wminj_ab(i1,i,j1,j) = Tiijj(i1,i,j1,j) 
#
      ENDPARDO i, j, i1, j1 
      execute sip_barrier 
#
      PARDO a, i, b, j  
#
            R1aibj(a,i,b,j) = 0.0 
#
            DO i1   
            DO j1  
#
               REQUEST Tau_ab(a,i1,b,j1) j1  
               GET Wminj_ab(i1,i,j1,j) 
#
               Taibj(a,i,b,j)   = Tau_ab(a,i1,b,j1)*Wminj_ab(i1,i,j1,j) 
               R1aibj(a,i,b,j) += Taibj(a,i,b,j)  
#
            ENDDO j1  
            ENDDO i1  
#
            R1aibj(a,i,b,j)           *= 0.5  
            PREPARE T2new_ab(a,i,b,j) += R1aibj(a,i,b,j) 
#
      ENDPARDO a, i, b, j  
      execute sip_barrier 
      delete Wminj_ab 
#
      ENDPROC WAEBFAB 
#     --------------- 
#
# PPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPP
#
      PROC WAEBF 
# 
           CALL WAEBFAA 
           CALL WAEBFAB 
           CALL WAEBFBB 
#
      ENDPROC WAEBF 
#
#
# -------------------------------------------------------------------------------  
#
# ------------------------------------------------------------------------ 
#
      PROC WMEBJAA 
#     ------------
#
# ---------------------------- 
# Initialize the intermediates 
# ---------------------------- 
#
      PARDO a, a1, i, i1  
#
               REQUEST              Viaai(i1,a1,a,i) i  
               REQUEST              Vaaii(a,a1,i1,i) i  
               REQUEST              e5aiai(a1,i1,a,i) i 
#
               TYaiai(a1,i1,a,i) = 0.
#
               DO i2
#
                  REQUEST              VSpipi(a1,i1,i,i2) i2
                 #GET                  t1a_old(a,i2)
#
                  T1aiai(a1,i1,a,i)  = VSpipi(a1,i1,i,i2)*Lt1aR(a,i2)
                  TYaiai(a1,i1,a,i) -= T1aiai(a1,i1,a,i)
#
               ENDDO i2

               Taiai(a1,i1,a,i)           = Vaaii(a,a1,i1,i)
               TYaiai(a1,i1,a,i)         -= Taiai(a1,i1,a,i)
               Taiai(a1,i1,a,i)           = Viaai(i1,a1,a,i)
               TYaiai(a1,i1,a,i)         += Taiai(a1,i1,a,i)
               TYaiai(a1,i1,a,i)         += e5aiai(a1,i1,a,i)
               PREPARE TAUP_aa(a1,i1,a,i) = TYaiai(a1,i1,a,i) 
#
      ENDPARDO a, a1, i, i1  
#
      PARDO j, b, j1, b1
#
               REQUEST Vjbbj(j1,b1,b,j) j  
               REQUEST Vbbjj(b,b1,j1,j) j  
               REQUEST e5bjbj(b1,j1,b,j) j  
#
               TYbjbj(b1,j1,b,j) = 0.
#
               DO j2  
#
                  REQUEST              VSqjqj(b1,j1,j,j2) j2  
                 #GET                  t1b_old(b,j2) 
#
                  T1bjbj(b1,j1,b,j)  = VSqjqj(b1,j1,j,j2)*Lt1bR(b,j2)  
                  TYbjbj(b1,j1,b,j) -= T1bjbj(b1,j1,b,j)  
#
               ENDDO j2  
#
               T1bjbj(b1,j1,b,j)          = Vbbjj(b,b1,j1,j) 
               TYbjbj(b1,j1,b,j)         -= T1bjbj(b1,j1,b,j)  
               T1bjbj(b1,j1,b,j)          = Vjbbj(j1,b1,b,j) 
               TYbjbj(b1,j1,b,j)         += T1bjbj(b1,j1,b,j)
               TYbjbj(b1,j1,b,j)         += e5bjbj(b1,j1,b,j)  
               PREPARE TAUP_bb(b1,j1,b,j) = TYbjbj(b1,j1,b,j) 
#
      ENDPARDO j, b, j1, b1
#
      PARDO j, b, i, a
#
               REQUEST           Viabj(i,a,b,j) j  
               REQUEST           e5aibj(a,i,b,j) a 
#
               Taibj(a,i,b,j) = 0.
#
               DO j1
                  REQUEST           Vpiqj(a,i,j,j1) j1  
                 #GET               t1b_old(b,j1) 
#
                  T1aibj(a,i,b,j) = Vpiqj(a,i,j,j1)*Lt1bR(b,j1) 
                  Taibj(a,i,b,j) -= T1aibj(a,i,b,j) 
               ENDDO j1

               T1aibj(a,i,b,j)          = Viabj(i,a,b,j) 
               Taibj(a,i,b,j)          += T1aibj(a,i,b,j)
               Taibj(a,i,b,j)          += e5aibj(a,i,b,j) 
               PREPARE TAUP_ab(a,i,b,j) = Taibj(a,i,b,j) 
#
      ENDPARDO j, b, i, a
#
      PARDO i, a, j, b
#
               REQUEST           Viabj(i,a,b,j) j  
               REQUEST           e6aibj(a,i,b,j) a 
#
               Taibj(a,i,b,j) = 0.
#
               DO i1
                  REQUEST           Vjbii(j,b,i1,i) i  
                 #GET               t1a_old(a,i1) 
#
                  T1aibj(a,i,b,j) = Vjbii(j,b,i1,i)*Lt1aR(a,i1) 
                  Taibj(a,i,b,j) -= T1aibj(a,i,b,j) 
               ENDDO i1
#
               T1aibj(a,i,b,j)           = Viabj(i,a,b,j) 
               Taibj(a,i,b,j)           += T1aibj(a,i,b,j) 
               Taibj(a,i,b,j)           += e6aibj(a,i,b,j) 
#
               PREPARE TAUP1_ab(a,i,b,j) = Taibj(a,i,b,j) 
#
      ENDPARDO i, a, j, b
#
# --------------------------------- 
# Done Initialize the intermediates 
# --------------------------------- 
#
      execute server_barrier 
#
# --------------------------------- 
# Finalize the intermediates 
# --------------------------------- 
#
      PARDO a, i, a2, i2
#
         REQUEST              T2old_aa(a2,i,a,i2) i2  
        #GET                  t1a_old(a2,i) 
        #GET                  t1a_old(a,i2) 
 
         tai(a,i2)          = Lt1aR(a,i2) 
         T1aiai(a2,i2,a,i)  = Lt1aR(a2,i)^tai(a,i2) 
         T2aiai(a2,i2,a,i)  = t2old_aa(a2,i,a,i2) 
         T2aiai(a2,i2,a,i) *= 0.5 
         T2aiai(a2,i2,a,i) += T1aiai(a2,i2,a,i) 

         DO i1
         DO a1 
#
            REQUEST                       VSpipi(a1,i1,a2,i2) i2  
            Tyaiai(a1,i1,a,i)           = 0.
            T3aiai(a1,i1,a,i)           = VSpipi(a1,i1,a2,i2)*T2aiai(a2,i2,a,i) 
            TYaiai(a1,i1,a,i)          -= T3aiai(a1,i1,a,i)  
            PREPARE TAUP_aa(a1,i1,a,i) += TYaiai(a1,i1,a,i)
#
         ENDDO a1
         ENDDO i1
#
      ENDPARDO a, i, a2, i2 

      PARDO a,i,b,j 
#
         REQUEST T2old_ab(a,i,b,j) j 
#
         DO i1
         DO a1 
            REQUEST Vpiqj(a1,i1,b,j) j 
#
            T1aiai(a1,i1,a,i)  = Vpiqj(a1,i1,b,j)*T2old_ab(a,i,b,j)
            T1aiai(a1,i1,a,i) *= 0.5

            PREPARE TAUP_aa(a1,i1,a,i) += T1aiai(a1,i1,a,i)
         ENDDO a1
         ENDDO i1
#
      ENDPARDO a,i,b,j

      PARDO b,j,b2,j2 

         REQUEST              T2old_bb(b2,j,b,j2) j2  
        #GET                  t1b_old(b2,j) 
        #GET                  t1b_old(b,j2) 
#
         tbj(b,j2)          = Lt1bR(b,j2) 
         T1bjbj(b2,j2,b,j)  = Lt1bR(b2,j)^tbj(b,j2) 
         T2bjbj(b2,j2,b,j)  = t2old_bb(b2,j,b,j2) 
         T2bjbj(b2,j2,b,j) *= 0.5 
         T2bjbj(b2,j2,b,j) += T1bjbj(b2,j2,b,j) 
#
         DO j1
         DO b1 
#
            REQUEST              VSqjqj(b1,j1,b2,j2) j2  
#
            TYbjbj(b1,j1,b,j) = 0. 
#
            T3bjbj(b1,j1,b,j)  = VSqjqj(b1,j1,b2,j2)*T2bjbj(b2,j2,b,j) 
            TYbjbj(b1,j1,b,j) -= T3bjbj(b1,j1,b,j)  
#
            PREPARE TAUP_bb(b1,j1,b,j) += TYbjbj(b1,j1,b,j) 
#
         ENDDO b1  
         ENDDO j1
#
      ENDPARDO b,j,b2,j2

      PARDO b,j,a,i
               
               REQUEST              T2old_ab(a,i,b,j) j

               DO b1 
               DO j1 
#
                  REQUEST              Vpiqj(a,i,b1,j1) j1
#
                  T1bjbj(b1,j1,b,j)  = Vpiqj(a,i,b1,j1)*T2old_ab(a,i,b,j)
                  T1bjbj(b1,j1,b,j) *= 0.5
#
                  PREPARE TAUP_bb(b1,j1,b,j) += T1bjbj(b1,j1,b,j)
#
               ENDDO j1
               ENDDO b1
 
      ENDPARDO b,j,a,i

      PARDO j,b,j1,b1
#
               REQUEST              t2old_bb(b1,j,b,j1) j1 
              #GET                  t1b_old(b1,j) 
              #GET                  t1b_old(b,j1) 
# 
               tbj(b,j1)          = Lt1bR(b,j1) 
               T2bjbj(b1,j,b,j1)  = Lt1bR(b1,j)^tbj(b,j1) 
               T1bjbj(b1,j,b,j1)  = 0.5*T2old_bb(b1,j,b,j1) 
               T1bjbj(b1,j,b,j1) += T2bjbj(b1,j,b,j1) 
#
               DO i  
               DO a 
#
                  REQUEST              Vpiqj(a,i,b1,j1) j1  
#
                  Taibj(a,i,b,j)  = 0.
                  T1aibj(a,i,b,j) = T1bjbj(b1,j,b,j1)*Vpiqj(a,i,b1,j1) 
                  Taibj(a,i,b,j)  -= T1aibj(a,i,b,j) 
#   
                  PREPARE TAUP_ab(a,i,b,j) += Taibj(a,i,b,j)
               ENDDO a 
               ENDDO i  
#
      ENDPARDO j,b,j1,b1
#
      PARDO b,j,a1,i1
#
         REQUEST            T2old_ab(a1,i1,b,j) j      
#
         DO a
         DO i
#
            REQUEST            VSpipi(a1,i1,a,i) i
#
            T1aibj(a,i,b,j)  = T2old_ab(a1,i1,b,j)*VSpipi(a1,i1,a,i)
            T1aibj(a,i,b,j) *= 0.5
            PREPARE TAUP_ab(a,i,b,j) += T1aibj(a,i,b,j)
#
         ENDDO i
         ENDDO a
#
      ENDPARDO b,j,a1,i1

      PARDO a,i,a1,i1
#
               REQUEST              T2old_aa(a1,i,a,i1) i1  
              #GET                  t1a_old(a1,i) 
              #GET                  t1a_old(a,i1) 
#
               tai(a,i1)          = Lt1aR(a,i1) 
               T2aiai(a,i,a1,i1)  = Lt1aR(a1,i)^tai(a,i1) 
               T1aiai(a,i,a1,i1)  = t2old_aa(a1,i,a,i1) 
               T1aiai(a,i,a1,i1) *= 0.5  
               T1aiai(a,i,a1,i1) += T2aiai(a,i,a1,i1) 
#
               DO j 
               DO b
#
                  REQUEST              Vpiqj(a1,i1,b,j) j  
#
                  Taibj(a,i,b,j)     = 0.
                  T1aibj(a,i,b,j)    = T1aiai(a,i,a1,i1)*Vpiqj(a1,i1,b,j) 
                  Taibj(a,i,b,j)    -= T1aibj(a,i,b,j) 
#
                  PREPARE TAUP1_ab(a,i,b,j) += Taibj(a,i,b,j)
               ENDDO  b
               ENDDO j
#
      ENDPARDO a,i,a1,i1
#
      PARDO a, i, b1, j1
#
         REQUEST            T2old_ab(a,i,b1,j1) j1
#
         DO b
         DO j
            REQUEST            VSqjqj(b1,j1,b,j) j
#
            T1aibj(a,i,b,j)  = T2old_ab(a,i,b1,j1)*VSqjqj(b1,j1,b,j)
            T1aibj(a,i,b,j) *= 0.5
#
            PREPARE TAUP1_ab(a,i,b,j) += T1aibj(a,i,b,j )
#
         ENDDO j
         ENDDO b
#
      ENDPARDO a, i, b1, j1
#
# --------------------------------- 
# Done Finalize the intermediates 
# --------------------------------- 
#
      create D2new_aa 
      execute server_barrier
#
# Contributions to T2new_aa 
# ------------------------- 
#
      PARDO a, i, a1, i1  
            REQUEST TAUP_aa(a1,i1,a,i)    i1 
#
            DO i2 
            DO a2 
               REQUEST                    T2old_aa(a2,i2,a1,i1) i1  
               R1aiai(a2,i2,a,i)        = T2old_aa(a2,i2,a1,i1)*TAUP_aa(a1,i1,a,i) 
               PUT D2new_aa(a2,i2,a,i) += R1aiai(a2,i2,a,i) 
            ENDDO a2 
            ENDDO i2 
#
      ENDPARDO a, i, a1, i1  
      execute server_barrier 
#
      PARDO a, i, a2, i2 
            GET                            D2new_aa(a2,i2,a,i) 
            R1aiai(a2,i2,a,i)            = D2new_aa(a2,i2,a,i) 
            R2aiai(a2,i,a,i2)            = R1aiai(a2,i2,a,i)
            R2aiai(a2,i,a,i2)           *= -1.0
            R3aiai(a,i2,a2,i)            = R2aiai(a2,i,a,i2)
            R4aiai(a,i,a2,i2)            = R1aiai(a2,i2,a,i)
#
            PREPARE T2new_aa(a2,i2,a,i) += R1aiai(a2,i2,a,i) 
            PREPARE T2new_aa(a2,i,a,i2) += R2aiai(a2,i,a,i2) 
            PREPARE T2new_aa(a,i2,a2,i) += R3aiai(a,i2,a2,i) 
            PREPARE T2new_aa(a,i,a2,i2) += R4aiai(a,i,a2,i2) 
      ENDPARDO a, i, a2, i2 
#
# Contributions to T2new_ab 
# ------------------------- 
#
      PARDO a, i, a1, i1 
#
            REQUEST            TAUP_aa(a1,i1,a,i)  i 
#
            DO j  
            DO b
#
               REQUEST            T2old_ab(a1,i1,b,j) j  
#
               R1aibj(a,i,b,j)  = TAUP_aa(a1,i1,a,i)*T2old_ab(a1,i1,b,j) 
               PREPARE T2new_ab(a,i,b,j) += R1aibj(a,i,b,j)
            ENDDO b  
            ENDDO j  
#
      ENDPARDO a, i, a1, i1 
      execute server_barrier 
      delete D2new_aa 
#
      ENDPROC WMEBJAA 
#     ---------------
#
# PPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPP 
#
# PPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPP 
#
      PROC WMEBJBB 
#     ------------
      create D2new_bb 
      execute server_barrier 
#
# Contributions to T2new_bb 
# ------------------------- 
#
      PARDO j, b, b1, j1 
#
            REQUEST TAUP_bb(b1,j1,b,j)    j1  
#
            DO j2
            DO b2
               REQUEST                    T2old_bb(b2,j2,b1,j1) j1  
               R1bjbj(b2,j2,b,j)        = T2old_bb(b2,j2,b1,j1)*TAUP_bb(b1,j1,b,j) 
               PUT D2new_bb(b2,j2,b,j) += R1bjbj(b2,j2,b,j) 
            ENDDO b2
            ENDDO j2 
#
      ENDPARDO j, b, b1, j1 
      execute server_barrier 
#
      PARDO b, j, b2, j2 
            GET                            D2new_bb(b2,j2,b,j) 
#
            R1bjbj(b2,j2,b,j)            = D2new_bb(b2,j2,b,j) 
            R2bjbj(b2,j,b,j2)            = R1bjbj(b2,j2,b,j) 
            R2bjbj(b2,j,b,j2)           *= -1.0  
            R3bjbj(b,j2,b2,j)            = R1bjbj(b2,j2,b,j) 
            R3bjbj(b,j2,b2,j)           *= -1.0  
            R4bjbj(b,j,b2,j2)            = R1bjbj(b2,j2,b,j) 
#
            PREPARE T2new_bb(b2,j2,b,j) += R1bjbj(b2,j2,b,j) 
            PREPARE T2new_bb(b2,j,b,j2) += R2bjbj(b2,j,b,j2) 
            PREPARE T2new_bb(b,j2,b2,j) += R3bjbj(b,j2,b2,j) 
            PREPARE T2new_bb(b,j,b2,j2) += R4bjbj(b,j,b2,j2) 
      ENDPARDO b, j, b2, j2 
#
# Contributions to T2new_ab 
# ------------------------- 
#
      PARDO a, i, b1, j1 
#
            REQUEST            T2old_ab(a,i,b1,j1) j1  
#
            DO b  
            DO j  
#
               REQUEST            TAUP_bb(b1,j1,b,j)  j1  
#
               R1aibj(a,i,b,j)  = T2old_ab(a,i,b1,j1)*TAUP_bb(b1,j1,b,j) 
               PREPARE T2new_ab(a,i,b,j) += R1aibj(a,i,b,j)
            ENDDO j  
            ENDDO b  
#
      ENDPARDO a, i, b1, j1 
      execute server_barrier 
      delete D2new_bb 
#
      ENDPROC WMEBJBB 
